A,QAAA,YAAA,EAAA;AAHA,iEAIA,GAJA,uDAKA,SALA,yDAMA,GANA,yDAQA,OARA;AAYA,KA3KA;AA6KA;AACA,IAAA,YA9KA,0BA8KA;AACA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,SAAA,GAAA,IAAA,IAAA,CAAA,SAAA,CAAA,SAAA,EAAA;AACA,WAAA,GAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA,KAnLA;AAqLA;AACA,IAAA,eAtLA,6BAsLA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,GAAA,CAAA,MAAA,CAAA,KAAA,SAAA;AACA,KAzLA;AA2LA;AACA,IAAA,YA5LA,0BA4LA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,WAAA,YAAA,GAAA,IAAA,IAAA,CAAA,SAAA,CAAA,OAAA,EAAA;AACA,WAAA,GAAA,CAAA,GAAA,CAAA,KAAA,YAAA;AACA,KAhMA;AAkMA;AACA,IAAA,eAnMA,6BAmMA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,WAAA,GAAA,CAAA,MAAA,CAAA,KAAA,YAAA;AACA,KAtMA;AAwMA,IAAA,QAxMA,sBAwMA;AACA,MAAA,QAAA,CAAA,MAAA,GADA,CAEA;AACA;AA3MA;AAlFA,CAAA","sourcesContent":["<template>\r\n  <!-- 地图量算 -->\r\n  <div class=\"measure-map\">\r\n    <div class=\"map-box\">\r\n      <div class=\"map-buttons\" style=\"padding: 15px\">\r\n\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 160px\"\r\n          v-model=\"adddate\"\r\n        ></el-input>\r\n\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"inputName\"\r\n          placeholder=\"停车场名称\"\r\n        ></el-input>\r\n\r\n\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 100px\"\r\n          v-model=\"parkadmin\"\r\n          placeholder=\"负责人\"\r\n        ></el-input>\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"parktel\"\r\n          placeholder=\"负责人联系方式\"\r\n        ></el-input>\r\n        <span>车位数：</span>\r\n        <el-input-number\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"parknum\"\r\n          :min=\"10\"\r\n          placeholder=\"车位数量\"\r\n        ></el-input-number>\r\n        <span>收费标准(元/时)：</span>\r\n        <el-input-number\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"parkmoney\"\r\n          placeholder=\"收费标准\"\r\n          :min=\"1\"\r\n        ></el-input-number>\r\n<br>\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          style=\"color: #5f8d4e\"\r\n          @click=\"addPolgy\"\r\n          class=\"map-btn map-element\"\r\n          >{{ pagType === \"add\" ? \"添加停车场\" : \"确认编辑\" }}</el-button\r\n        >\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"addRoadLayer\"\r\n          v-show=\"!roadNetwork\"\r\n          class=\"map-btn map-element\"\r\n          >打开路网</el-button\r\n        >\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"removeRoadLayer\"\r\n          v-show=\"roadNetwork\"\r\n          class=\"map-btn map-element\"\r\n          style=\"margin-left: 0\"\r\n          >关闭路网</el-button\r\n        >\r\n\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"addSatellite\"\r\n          v-show=\"isOrdMap\"\r\n          class=\"map-btn map-element\"\r\n          >卫星地图</el-button\r\n        >\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"removeSatellite\"\r\n          v-show=\"!isOrdMap\"\r\n          class=\"map-btn map-element\"\r\n          >普通地图</el-button\r\n        >\r\n\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"resetMap\"\r\n          class=\"map-btn map-element\"\r\n          >复位</el-button\r\n        >\r\n      </div>\r\n      <div id=\"container\" class=\"container\"></div>\r\n      <div class=\"input-card\" style=\"width: 200px\">\r\n        <h4 style=\"margin-bottom: 10px; font-weight: 600\">\r\n          停车场区域范围：\r\n        </h4>\r\n        <button\r\n          class=\"btn\"\r\n          v-if=\"pagType === 'add'\"\r\n          @click=\"drawPolygon\"\r\n          style=\"margin-bottom: 5px\"\r\n        >\r\n          绘制区域\r\n        </button>\r\n\r\n        <button\r\n          class=\"btn\"\r\n          v-if=\"pagType !== 'add'\"\r\n          @click=\"startEdit\"\r\n          style=\"margin-bottom: 5px\"\r\n        >\r\n          开始编辑\r\n        </button>\r\n        <button\r\n          class=\"btn\"\r\n          v-if=\"pagType !== 'add'\"\r\n          @click=\"endEdit\"\r\n          style=\"margin-bottom: 5px\"\r\n        >\r\n          结束编辑\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script type=\"text/javascript\" src=\"https://webapi.amap.com/maps?v=1.4.15&key=84f61226c5c3819011a303ef5c321e5f&plugin=AMap.MouseTool,AMap.HawkEye,AMap.DistrictSearch,AMap.Scale,AMap.ToolBar,AMap.ControlBar,AMap.ImageLayer\"></script>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      map: null,\r\n      mouseTool: null,\r\n      radios: null,\r\n\r\n      isOrdMap: true, //判断是卫星图还是普通图，普通图为true\r\n      satellite: null, //用来存储卫星地图影像\r\n      roadNetwork: false, //是否具有路网，默认没有\r\n      roadNetLayer: null, //用来存储路网\r\n\r\n      polyEditor: null,\r\n\r\n      // 停车场数据存储\r\n      mianji: 0,\r\n      pathArr: null,\r\n      inputName: null,\r\n      adddate:'',\r\n      parknum:50,\r\n      parkmoney:1,\r\n      parkadmin:'',\r\n      parktel:'',\r\n      valueType: null,\r\n\r\n      pagType: null,\r\n\r\n      dataList: null,\r\n      polygons: [], //覆盖物数组\r\n      oneData: null,\r\n      polyEditor: null, //编辑多边形\r\n      isEditIng: false,\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    console.log(this.$route);\r\n\r\n    const now = new Date();\r\n    const year = now.getFullYear();\r\n    const month = now.getMonth() + 1; // 月份从0开始，需要加1\r\n    const day = now.getDate();\r\n    const hours = now.getHours(); // 获取当前小时，例如：15\r\n    const minutes = now.getMinutes(); // 获取当前分钟，例如：30\r\n    const seconds = now.getSeconds(); // 获取当前秒数，例如：45\r\n    console.log(year, month, day);\r\n    this.adddate =\r\n      year +\r\n      \"/\" +\r\n      month +\r\n      \"/\" +\r\n      day +\r\n      \" \" +\r\n      hours +\r\n      \":\" +\r\n      minutes +\r\n      \":\" +\r\n      seconds;\r\n\r\n    this.pagType = this.$route.params.editid;\r\n    if (this.pagType !== \"add\") {\r\n      var res = null;\r\n      res = await this.$axios.post(\"/LvDiDataTableGetOne/LvDiDataTableGetOne\", {\r\n        _id: this.pagType,\r\n      });\r\n\r\n      this.oneData = res.data.data[0];\r\n      this.inputName = this.oneData.name;\r\n      this.valueType = this.oneData.type;\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap();\r\n\r\n    if (this.pagType === \"add\") {\r\n      this.addPol();\r\n    } else {\r\n      this.addEditPol();\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    initMap() {\r\n      //初始化地图\r\n      this.map = new AMap.Map(\"container\", {\r\n        resizeEnable: true,\r\n        zoom: 14, //级别\r\n        zooms: [3, 30],\r\n        center: [118.18,39.63], //中心点坐标\r\n\r\n        // center: [116.471354, 39.994257],\r\n        // zoom: 16.8,\r\n\r\n        viewMode: \"3D\", //使用3D视图\r\n        lang: this.lang,\r\n        mapStyle: \"amap://styles/macaron\",\r\n        infoWindow: null,\r\n      });\r\n      this.mouseTool = new AMap.MouseTool(this.map);\r\n\r\n      this.mouseTool.on(\"draw\", (event) => {\r\n        // event.obj 为绘制出来的覆盖物对象\r\n        // console.log(\"覆盖物对象绘制完成\");\r\n        // console.log(event.obj.getArea(), event.obj.getPath());\r\n        this.mianji = event.obj.getArea();\r\n        this.pathArr = event.obj.getPath();\r\n      });\r\n    },\r\n\r\n    async addEditPol() {\r\n      var res = null;\r\n      res = await this.$axios.post(\"/LvDiDataTableGetOne/LvDiDataTableGetOne\", {\r\n        _id: this.pagType,\r\n      });\r\n\r\n      var zb = res.data.data[0].path.map((ele) => [ele.R, ele.Q]);\r\n\r\n      var polygon = new AMap.Polygon({\r\n        path: zb,\r\n        strokeColor: \"#FF33FF\",\r\n        strokeWeight: 6,\r\n        strokeOpacity: 0.2,\r\n        fillOpacity: 0.4,\r\n        fillColor: \"#1791fc\",\r\n        zIndex: 50,\r\n      });\r\n\r\n      this.map.add(polygon);\r\n      this.map.setFitView([polygon]);\r\n\r\n      this.polyEditor = new AMap.PolyEditor(this.map, polygon);\r\n\r\n      this.polyEditor.on(\"end\", (event) => {\r\n        // log.info(\"触发事件： end\");\r\n        console.log(\"end\", event);\r\n        this.mianji = event.target.getArea();\r\n        this.pathArr = event.target.getPath();\r\n        // event.target 即为编辑后的多边形对象\r\n      });\r\n    },\r\n\r\n    startEdit() {\r\n      this.isEditIng = true;\r\n      this.polyEditor.open();\r\n    },\r\n\r\n    endEdit() {\r\n      this.isEditIng = false;\r\n      this.polyEditor.close();\r\n    },\r\n\r\n    async addPol() {\r\n      var res = await this.$axios.get(\r\n        \"/LvDiDataTableGetData/LvDiDataTableGetData\"\r\n      );\r\n      this.dataList = res.data.data;\r\n\r\n      for (let i = 0; i < this.dataList.length; i++) {\r\n        var path = this.dataList[i].path.map((ele) => [ele.R, ele.Q]);\r\n\r\n        var colorType = {\r\n          strokeColor: \"#FF33FF\",\r\n          strokeWeight: 6,\r\n          strokeOpacity: 0.2,\r\n          fillOpacity: 0.4,\r\n          fillColor: \"#1791fc\",\r\n        };\r\n\r\n        let polygon = new AMap.Polygon({\r\n          path: path,\r\n          ...colorType,\r\n        });\r\n        this.polygons.push(polygon);\r\n      }\r\n\r\n      this.map.add(this.polygons);\r\n    },\r\n\r\n    async addPolgy() {\r\n      if (!this.mianji) {\r\n        this.$message.error(\"请先绘制停车场范围\");\r\n      } else {\r\n        if (this.parkadmin && this.inputName&& this.parktel) {\r\n          if (this.pagType === \"add\") {\r\n            var adddata={\r\n              name: this.inputName,\r\n              path: this.pathArr,\r\n              mianji: this.mianji,\r\n              adddate:this.adddate,\r\n              parknum:this.parknum,\r\n              parkmoney:this.parkmoney,\r\n              parkadmin:this.parkadmin,\r\n              parktel:this.parktel,\r\n              pl:[]\r\n            }\r\n            console.log('添加数据',adddata)\r\n            var res = null;\r\n            res = await this.$axios.post(\r\n              \"/TSParkDataAdd/TSParkDataAdd\",\r\n              adddata\r\n            );\r\n            if (res.status === 200) {\r\n              this.$message({\r\n                message: \"停车场信息添加成功\",\r\n                type: \"success\",\r\n              });\r\n              this.$router.push(\"/data/manage\");\r\n            }\r\n          } else {\r\n            // 提交编辑\r\n\r\n            var res2 = null;\r\n            res2 = await this.$axios.post(\r\n              \"/LvDiDataTableEditData/LvDiDataTableEditData\",\r\n              {\r\n                name: this.inputName,\r\n                type: this.valueType,\r\n                path: this.pathArr,\r\n                mianji: this.mianji,\r\n                _id: this.pagType,\r\n              }\r\n            );\r\n            // console.log();\r\n            if (res2.status === 200) {\r\n              this.$message({\r\n                message: \"停车场信息编辑成功\",\r\n                type: \"success\",\r\n              });\r\n\r\n              this.$router.push(\"/data/manage\");\r\n            }\r\n          }\r\n        } else {\r\n          this.$message.error(\"请完善停车场信息\");\r\n        }\r\n      }\r\n    },\r\n\r\n    drawPolygon() {\r\n      this.map.clearMap();\r\n      this.mouseTool.polygon({\r\n        strokeColor: \"#285430\",\r\n        strokeOpacity: 1,\r\n        strokeWeight: 6,\r\n        strokeOpacity: 0.2,\r\n        fillColor: \"#A4BE7B\",\r\n        fillOpacity: 0.4,\r\n        // 线样式还支持 'dashed'\r\n        strokeStyle: \"solid\",\r\n        // strokeStyle是dashed时有效\r\n        // strokeDasharray: [30,10],\r\n      });\r\n    },\r\n\r\n    // 打开卫星地图\r\n    addSatellite() {\r\n      // console.log(111);\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.satellite = new AMap.TileLayer.Satellite();\r\n      this.map.add(this.satellite);\r\n    },\r\n\r\n    // 关闭卫星地图\r\n    removeSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.map.remove(this.satellite);\r\n    },\r\n\r\n    // 打开路网\r\n    addRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.roadNetLayer = new AMap.TileLayer.RoadNet();\r\n      this.map.add(this.roadNetLayer);\r\n    },\r\n\r\n    // 关闭路网\r\n    removeRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.map.remove(this.roadNetLayer);\r\n    },\r\n\r\n    resetMap() {\r\n      location.reload();\r\n      // this.map.setCenter([115.464523, 38.874476]);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.btn {\r\n  width: 6rem;\r\n  margin: 0 1rem 0 4.5rem;\r\n  /* color: #1791fc; */\r\n}\r\n\r\n/* 弹框start */\r\nhtml {\r\n  font-size: 12px;\r\n}\r\n.amap-copyright {\r\n  box-sizing: content-box;\r\n}\r\n* {\r\n  box-sizing: border-box;\r\n}\r\n.input-textarea {\r\n  color: grey;\r\n  height: 8em;\r\n  overflow: auto;\r\n  border-radius: 0.4rem;\r\n  border: 1px solid #ced4da;\r\n  margin-bottom: 1rem;\r\n}\r\nbody {\r\n  margin: 0;\r\n  font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto,\r\n    \"Helvetica Neue\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\",\r\n    \"Segoe UI Symbol\", \"Noto Color Emoji\";\r\n  line-height: 1.5;\r\n  font-weight: 300;\r\n  color: #111213;\r\n}\r\n\r\nhr {\r\n  margin: 0.5rem 0;\r\n  box-sizing: content-box;\r\n  height: 0;\r\n  overflow: visible;\r\n  border: 0;\r\n  border-top: 1px solid rgba(0, 0, 0, 0.1);\r\n}\r\n\r\np {\r\n  margin-top: 0;\r\n  margin-bottom: 0;\r\n}\r\n\r\nlabel {\r\n  display: inline-block;\r\n  margin-bottom: 0.4rem;\r\n}\r\n\r\nlabel,\r\n.btn {\r\n  margin-left: 0;\r\n  font-size: 1rem;\r\n}\r\n\r\nbutton,\r\ninput,\r\nselect {\r\n  margin: 0;\r\n  font-family: inherit;\r\n  font-size: inherit;\r\n  line-height: inherit;\r\n  overflow: visible;\r\n  text-transform: none;\r\n}\r\n\r\n[type=\"button\"]::-moz