IAAA,EAAA,IAAA,uCAAA,IAAA;AAbA;;AAMA,qBAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,MAAA,CAAA,QAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AAAA,wBAAA,CAAA;AAQA,iBAdA,CAeA;;;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,IAAA,IAAA,CAAA,YAAA,CAAA;AACA,kBAAA,GAAA,EAAA,MAAA,CAAA,GADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA,CAAA;AAIA,gBAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,MAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,MAAA,CAAA,IAAA,EAAA;AACA,kBAAA,MAAA,EAAA,YADA;AAEA,kBAAA,KAAA,EAAA;AAFA,iBAAA,EArBA,CA0BA;;;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA;AACA,kBAAA,KAAA,EAAA;AACA,oBAAA,MAAA,EAAA,EADA;AAEA,oBAAA,KAAA,EAAA;AACA,2BAAA,SADA;AAEA,4BAAA,SAFA;AAGA,2BAAA,SAHA;AAIA,2BAAA,SAJA;AAKA,2BAAA;AALA;AAFA;AADA,iBAAA;;AAaA,gBAAA,MAAA,CAAA,KAAA,CAAA,MAAA;;AAxCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyCA;AAtFA;AA/CA,CAAA","sourcesContent":["<template>\r\n  <div\r\n    class=\"data-manage\"\r\n    v-loading=\"isPush\"\r\n    element-loading-text=\"热力图绘制中\"\r\n    element-loading-spinner=\"el-icon-loading\"\r\n    element-loading-background=\"rgba(0, 0, 0, 0.8)\"\r\n  >\r\n    <div class=\"data-manage-bottom padding15\">\r\n      <div\r\n        id=\"container0307\"\r\n        class=\"data-manage-buttons\"\r\n        style=\"\r\n          background: #fff;\r\n          border: 1px solid #ccc;\r\n          height: calc(100vh - 150px);\r\n        \"\r\n      ></div>\r\n\r\n      <div style=\"position: fixed; top: 76px; right: 15px\">\r\n        <div class=\"tablecard2\" style=\"width: 320px; z-index: 999\">\r\n          <div style=\"padding: 10px; box-sizing: border-box\">\r\n            <el-tag type=\"warning\" v-if=\"isFile\"\r\n              >浏览器存储已上传遥感文件</el-tag\r\n            >\r\n            <el-tag type=\"danger\" v-else>文件未上传</el-tag>\r\n          </div>\r\n\r\n          <div style=\"padding: 10px\">\r\n            <el-button type=\"primary\" @click=\"drawPoly\" v-if=\"isFile\"\r\n              >绘制茶园分布热力图</el-button\r\n            >\r\n            <br />\r\n            <br />\r\n            <el-button type=\"primary\" @click=\"fuwei\">复位地图</el-button>\r\n          </div>\r\n\r\n          <div style=\"padding: 10px\">\r\n            <h3>图例</h3>\r\n\r\n            <img style=\"height:250px\" src=\"../assets/hotlogo.jpg\" alt=\"\">\r\n          </div>\r\n        </div>\r\n        <el-card\r\n          shadow=\"never\"\r\n          class=\"box-card\"\r\n          style=\"width: 120px; z-index: 9999\"\r\n        >\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>图层切换</span>\r\n          </div>\r\n          <el-radio-group v-model=\"mapType\" @change=\"mapChange\">\r\n            <el-radio label=\"normal\">标准</el-radio><br />\r\n            <el-radio label=\"dark\">幻影黑</el-radio><br />\r\n            <el-radio label=\"light\">月光银</el-radio><br />\r\n            <el-radio label=\"yg\">遥感图</el-radio>\r\n          </el-radio-group>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nexport default {\r\n  computed: {\r\n    ...mapState([\"markData2\"]),\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      markData: [],\r\n      dialogTableVisible: false,\r\n\r\n      mapType: \"yg\",\r\n      map: \"\",\r\n      marker: null, //点位\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n      markers: [],\r\n      satellite: null,\r\n\r\n      isPG: false,\r\n      scelOption: {\r\n        scl1: \"杭州市\",\r\n        scl2: \"原始影像\",\r\n      },\r\n\r\n      dataList: [],\r\n      polygons: [],\r\n      pathArr: null,\r\n      isFile: false,\r\n      isPush: false,\r\n\r\n      list: [],\r\n      layer:null\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.changePngMap();\r\n    this.mapChange();\r\n  },\r\n\r\n  created() {\r\n    if (sessionStorage.getItem(\"isfiled\") === \"文件已上传\") {\r\n      this.isFile = true;\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    changeFile() {\r\n      console.log(\"文件上传\");\r\n      this.isFile = true;\r\n      sessionStorage.setItem(\"isfiled\", \"文件已上传\");\r\n    },\r\n\r\n    drawPoly() {\r\n      this.isPush = true;\r\n      setTimeout(() => {\r\n        this.isPush = false;\r\n        this.addPol();\r\n      }, 1000);\r\n    },\r\n\r\n    fuwei() {\r\n      this.map.setZoomAndCenter(13, [118.200716, 30.295263]);\r\n    },\r\n\r\n    mapChange() {\r\n      if (this.mapType === \"yg\") {\r\n        this.satellite = new AMap.TileLayer.Satellite();\r\n        this.map.add(this.satellite);\r\n      } else {\r\n        if (this.satellite) {\r\n          this.map.remove(this.satellite);\r\n        }\r\n        var styleName = \"amap://styles/\" + this.mapType;\r\n        this.map.setMapStyle(styleName);\r\n      }\r\n    },\r\n\r\n    changePngMap() {\r\n      this.map = new AMap.Map(\"container0307\", {\r\n        resizeEnable: true,\r\n        zoom: 14, //级别\r\n        zooms: [11, 30],\r\n        center: [118.200716, 30.295263], //中心点坐标\r\n        viewMode: \"2D\", //使用3D视图\r\n        mapStyle: \"amap://styles/normal\",\r\n        infoWindow: null,\r\n      });\r\n      this.mapType = \"yg\";\r\n    },\r\n\r\n    async addPol() {\r\n      var res = await this.$axios.get(\"/ChaYuanDataGetData/ChaYuanDataGetData\");\r\n      this.dataList = res.data.data;\r\n\r\n      this.list = [];\r\n\r\n      for (let i = 0; i < this.dataList.length; i++) {\r\n        var path = this.dataList[i].path.map((ele) => {\r\n          return {\r\n            coordinate: [ele.lng, ele.lat],\r\n          count: i,\r\n          };\r\n        });\r\n        this.list.push(...path);\r\n      }\r\n      // console.log(\" this.list\", this.list);\r\n        this.layer = new Loca.HeatmapLayer({\r\n        map: this.map,\r\n        fitView: true,\r\n      });\r\n      console.log(\"this.list\", this.list);\r\n      this.layer.setData(this.list, {\r\n        lnglat: \"coordinate\",\r\n        value: \"count\",\r\n      });\r\n\r\n      //  radius为热力半径，由于初期数据未录入，故设置的较大，默认为16，后续数据全部录入后再进行修改。\r\n      this.layer.setOptions({\r\n        style: {\r\n          radius: 25,\r\n          color: {\r\n            0.5: \"#2c7bb6\",\r\n            0.65: \"#abd9e9\",\r\n            0.7: \"#ffffbf\",\r\n            0.9: \"#fde468\",\r\n            1.0: \"#d7191c\",\r\n          },\r\n        },\r\n      });\r\n\r\n      this.layer.render();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.el-upload-dragger {\r\n  width: 300px;\r\n}\r\n\r\n.data-manage {\r\n  background: #f7f7f7;\r\n}\r\n\r\n.tablecard2 {\r\n  height: calc(100vh - 151px);\r\n  background: #fff;\r\n  border: 1px solid #ccc;\r\n  z-index: 999;\r\n}\r\n.box-card {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 322px;\r\n  border-radius: 0;\r\n}\r\n.box-card2 {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0px;\r\n  border-radius: 0;\r\n  height: 140px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}