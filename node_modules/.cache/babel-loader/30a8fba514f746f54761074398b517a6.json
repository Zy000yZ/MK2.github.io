A,OAAA,EAAA,EAtBA;AAwBA;AACA,MAAA,kBAAA,EAAA,QAzBA;AA0BA,MAAA,WAAA,EAAA,KA1BA;AA0BA;AACA,MAAA,SAAA,EAAA,IA3BA;AA4BA,MAAA,QAAA,EAAA,EA5BA;AA4BA;AACA,MAAA,iBAAA,EAAA,EA7BA;AA6BA;AACA,MAAA,WAAA,EAAA,IA9BA,CA8BA;AAEA;;AAhCA,KAAA;AAkCA,GAtCA;AAwCA,EAAA,QAAA,oBACA,QAAA,CAAA,CAAA,MAAA,EAAA,YAAA,CAAA,CADA,CAxCA;AA2CA,EAAA,OA3CA,qBA2CA,CACA;AACA;AACA;AACA;AACA,GAhDA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,SAAA,OAAA,GADA,CACA;AAEA;AACA;;AACA,SAAA,SAAA,GAAA,IAAA,IAAA,CAAA,SAAA,CAAA,KAAA,GAAA,CAAA,CALA,CAOA;AACA;AAEA;AACA,GA7DA;AA+DA,EAAA,OAAA,kCACA,YAAA,CAAA,CAAA,kBAAA,EAAA,eAAA,CAAA,CADA;AAGA,IAAA,OAHA,qBAGA;AAAA;;AACA;AACA,WAAA,GAAA,GAAA,IAAA,IAAA,CAAA,GAAA,CAAA,WAAA,EAAA;AACA,QAAA,YAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAFA;AAEA;AACA,QAAA,KAAA,EAAA,CAAA,GAAA,EAAA,EAAA,CAHA;AAIA,QAAA,MAAA,EAAA,CAAA,MAAA,EAAA,IAAA,CAJA;AAIA;AACA,QAAA,QAAA,EAAA,IALA;AAKA;AACA,QAAA,IAAA,EAAA,KAAA,IANA;AAOA,QAAA,QAAA,EAAA,qBAPA;AAQA,QAAA,UAAA,EAAA;AARA,OAAA,CAAA,CAFA,CAYA;AACA;;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,YAAA;AACA,YAAA,WAAA,GAAA,IAAA,IAAA,CAAA,WAAA,CAAA;AACA,UAAA,kBAAA,EAAA,IADA;AACA;AACA,UAAA,OAAA,EAAA,KAFA;AAEA;AACA,UAAA,QAAA,EAAA,IAHA;AAGA;AACA,UAAA,MAAA,EAAA,CAAA,EAAA,EAAA,EAAA,CAJA,CAIA;AACA;;AALA,SAAA,CAAA;;AAOA,QAAA,KAAA,CAAA,GAAA,CAAA,UAAA,CAAA,WAAA;;AACA,QAAA,WAAA,CAAA,kBAAA,CAAA,UAAA,MAAA,EAAA,MAAA,EAAA;AACA,cAAA,MAAA,IAAA,UAAA,EAAA;AACA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,MAAA;;AACA,YAAA,KAAA,CAAA,aAAA,CAAA,MAAA,CAAA,QAAA;AACA,WAJA,MAIA;AACA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA;AACA,cAAA,OAAA,EAAA,sBADA;AAEA,cAAA,IAAA,EAAA;AAFA,aAAA;AAIA;AACA,SAZA;AAaA,OAtBA;AAuBA,KAxCA;AA0CA,IAAA,cA1CA,4BA0CA;AAAA;;AACA;AAEA,UAAA,KAAA,UAAA,EAAA;AACA,YAAA,EAAA,GAAA,IAAA;AACA,aAAA,SAAA,CAAA,MAAA,CAAA,EAAA;AACA,aAAA,SAAA,CAAA,EAAA,CAAA,MAAA,EAAA,UAAA,CAAA,EAAA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,CAAA,CAAA,GAAA,CAAA,WAAA,EAAA,EAFA,CAEA;;AACA,UAAA,EAAA,GAAA,CAAA,CAAA,GAAA,CAAA,WAAA,EAAA;AAEA,cAAA,aAAA,GAAA,IAAA,IAAA,CAAA,OAAA,CAAA;AACA,YAAA,GAAA,EAAA,MAAA,CAAA,GADA;AAEA,YAAA,KAAA,EAAA;AAFA,WAAA,CAAA;AAKA,UAAA,aAAA,CAAA,MAAA,EACA;AACA,UAAA,MAAA,CAAA,UAFA,EAGA,IAAA,IAAA,CAAA,MAAA,CAAA,EAAA,CAAA,CAAA,EAAA,EAAA,CAAA,CAAA,CAHA,EAIA,UAAA,MAAA,EAAA,MAAA,EAAA,CAAA,CAJA;;AAOA,UAAA,MAAA,CAAA,SAAA,CAAA,KAAA,GAjBA,CAiBA;;AACA,SAlBA;AAmBA,OAtBA,MAsBA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,OAAA,EAAA,mBADA;AAEA,UAAA,IAAA,EAAA;AAFA,SAAA;AAIA;AACA,KAzEA;AA2EA,IAAA,UA3EA,sBA2EA,IA3EA,EA2EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,EADA,CAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,KAtFA;AAwFA;AAEA;AACA,IAAA,YA3FA,0BA2FA;AACA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,SAAA,GAAA,IAAA,IAAA,CAAA,SAAA,CAAA,SAAA,EAAA;AACA,WAAA,GAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA,KAhGA;AAkGA;AACA,IAAA,eAnGA,6BAmGA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,GAAA,CAAA,MAAA,CAAA,KAAA,SAAA;AACA,KAtGA;AAwGA;AACA,IAAA,YAzGA,0BAyGA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,WAAA,YAAA,GAAA,IAAA,IAAA,CAAA,SAAA,CAAA,OAAA,EAAA;AACA,WAAA,GAAA,CAAA,GAAA,CAAA,KAAA,YAAA;AACA,KA7GA;AA+GA;AACA,IAAA,eAhHA,6BAgHA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,WAAA,GAAA,CAAA,MAAA,CAAA,KAAA,YAAA;AACA,KAnHA;AAoHA;AACA,IAAA,YArHA,wBAqHA,IArHA,EAqHA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,cAAA;AACA,KAzHA;AA2HA,IAAA,WA3HA,uBA2HA,WA3HA,EA2HA,EA3HA,EA2HA;AACA,UAAA,WAAA,GAAA,KAAA,WAAA;AACA,UAAA,OAAA,GAAA,WAAA,GACA,WAAA,CAAA,MAAA,CAAA,KAAA,YAAA,CAAA,WAAA,CAAA,CADA,GAEA,WAFA,CAFA,CAKA;;AACA,MAAA,EAAA,CAAA,OAAA,CAAA;AACA,KAlIA;AAmIA,IAAA,YAnIA,wBAmIA,WAnIA,EAmIA;AACA,aAAA,UAAA,WAAA,EAAA;AACA,eACA,WAAA,CAAA,IAAA,CAAA,WAAA,GAAA,OAAA,CAAA,WAAA,CAAA,WAAA,EAAA,MACA,CAFA;AAIA,OALA;AAMA,KA1IA;AA4IA;AACA,IAAA,QA7IA,sBA6IA;AACA,MAAA,QAAA,CAAA,MAAA,GADA,CAEA;AACA,KAhJA;AAkJA,IAAA,iBAlJA,+BAkJA;AACA,WAAA,SAAA,CAAA,KAAA,CAAA,IAAA;AACA,WAAA,iBAAA,GAAA,EAAA;AACA,KArJA;AAsJA,IAAA,SAtJA,uBAsJA;AACA,WAAA,SAAA,CAAA,KAAA,CAAA,IAAA;AACA,WAAA,WAAA,GAAA,KAAA;AAEA,WAAA,iBAAA,GAAA,EAAA;AACA,KA3JA;AA6JA,IAAA,iBA7JA,+BA6JA;AACA,WAAA,WAAA,GAAA,CAAA,KAAA,WAAA;AACA,KA/JA,CAiKA;;AAjKA;AA/DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"heritage-industry\">\r\n    <!-- 地图容器 -->\r\n    <div class=\"map-buttons\">\r\n      <!-- 类型搜索start -->\r\n      <!-- <el-select\r\n        class=\"typeSelect\"\r\n        v-model=\"typeSelect\"\r\n        placeholder=\"请选择类型\"\r\n        size=\"mini\"\r\n      >\r\n        <el-option\r\n          v-for=\"item in options\"\r\n          :key=\"item.name\"\r\n          :label=\"item.name\"\r\n          :value=\"item.name\"\r\n        >\r\n        </el-option>\r\n      </el-select> -->\r\n      <!-- <el-button\r\n        id=\"selectBtn\"\r\n        class=\"selectBtn\"\r\n        size=\"mini\"\r\n        icon=\"el-icon-search\"\r\n        @click=\"searchTypeClick\"\r\n      ></el-button> -->\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addLastAddress\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"选择终点位置\" : \"Open road\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addRoadLayer\"\r\n        v-show=\"!roadNetwork\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"打开路网\" : \"Open road\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"removeRoadLayer\"\r\n        v-show=\"roadNetwork\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"关闭路网\" : \"Close road\" }}</el-button\r\n      >\r\n\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addSatellite\"\r\n        v-show=\"isOrdMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"卫星地图\" : \"Satellite map\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"removeSatellite\"\r\n        v-show=\"!isOrdMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"普通地图\" : \"General map\" }}</el-button\r\n      >\r\n\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"resetMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"复位\" : \"Reset\" }}</el-button\r\n      >\r\n    </div>\r\n    <div id=\"container\" class=\"container\">\r\n      <div id=\"panel\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script type=\"text/javascript\" src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"></script>\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\nexport default {\r\n  name: \"HeritageIndustry\",\r\n\r\n  data() {\r\n    return {\r\n      map: \"\",\r\n      district: null,\r\n      polygons: [],\r\n      zoom: 12,\r\n      marker: null, //工业遗产点位\r\n\r\n      // 弹框内容\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n\r\n      dataList: null, // 工业遗产数据\r\n      isOrdMap: true, //判断是卫星图还是普通图，普通图为true\r\n      satellite: null, //用来存储卫星地图影像\r\n      roadNetwork: false, //是否具有路网，默认没有\r\n      roadNetLayer: null, //用来存储路网\r\n\r\n      // 带有输入建议的搜索框\r\n      restaurants: [],\r\n      searchIndustry: \"\",\r\n      searchObj: null,\r\n      markers: [],\r\n\r\n      // 几何查询\r\n      geometrySearchType: \"circle\",\r\n      geometryBox: false, //几何查询面板隐藏展示\r\n      mouseTool: null,\r\n      overlays: [], //绘制好的覆盖物\r\n      geometrySearchRes: [], //几何查询结果\r\n      issearchRes: true, //查询结果图标显示隐藏\r\n\r\n      // 类型查询\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapState([\"lang\", \"nowAddress\"]),\r\n  },\r\n  created() {\r\n    // var res = await this.$axios.get(\"/getHeritageMainData/getHeritageMainData\");\r\n    // this.dataList = res.data.data;\r\n    // this.getAllType(); //初始化下拉菜单\r\n    // this.changeinstryType(\"one\");\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap(); //初始化地图\r\n\r\n    // this.disSearch();\r\n    // this.addMarker();\r\n    this.mouseTool = new AMap.MouseTool(this.map);\r\n\r\n    // console.log(\"test\", this.map.getZoom());\r\n    // this.mapZoom();\r\n\r\n    // console.log(\"返回所有覆盖物\", this.map.getAllOverlays(\"marker\"));\r\n  },\r\n\r\n  methods: {\r\n    ...mapMutations([\"changeinstryType\", \"getNowAddress\"]),\r\n\r\n    initMap() {\r\n      //初始化地图\r\n      this.map = new AMap.Map(\"container\", {\r\n        resizeEnable: true,\r\n        zoom: this.zoom, //级别\r\n        zooms: [6.5, 18],\r\n        center: [118.73, 32.0], //中心点坐标\r\n        viewMode: \"3D\", //使用3D视图\r\n        lang: this.lang,\r\n        mapStyle: \"amap://styles/fresh\",\r\n        infoWindow: null,\r\n      });\r\n      // console.log(111);\r\n      // 如若定位功能不可用，注释掉该段代码切换为固定位置\r\n      AMap.plugin(\"AMap.Geolocation\", () => {\r\n        var geolocation = new AMap.Geolocation({\r\n          enableHighAccuracy: true, //是否使用高精度定位，默认:true\r\n          timeout: 10000, //超过10秒后停止定位，默认：5s\r\n          position: \"RB\", //定位按钮的停靠位置\r\n          offset: [10, 20], //定位按钮与设置的停靠位置的偏移量，默认：[10, 20]\r\n          // zoomToAccuracy: true, //定位成功后是否自动调整地图视野到定位点\r\n        });\r\n        this.map.addControl(geolocation);\r\n        geolocation.getCurrentPosition((status, result) => {\r\n          if (status == \"complete\") {\r\n            // this.onComplete(result);\r\n            console.log(result);\r\n            this.getNowAddress(result.position);\r\n          } else {\r\n            // onError(result);\r\n            this.$message({\r\n              message: \"定位失败，请切换Eage浏览器再次尝试。\",\r\n              type: \"warning\",\r\n            });\r\n          }\r\n        });\r\n      });\r\n    },\r\n\r\n    addLastAddress() {\r\n      // console.log(this.typeSelect);\r\n\r\n      if (this.nowAddress) {\r\n        var zb = null;\r\n        this.mouseTool.marker({});\r\n        this.mouseTool.on(\"draw\", (e) => {\r\n          // this.overlays.push(e.obj);\r\n          console.log(\"e.obj---------\", e.obj.getPosition()); //获取点标记的位置\r\n          zb = e.obj.getPosition();\r\n\r\n          var drivingOption = new AMap.Driving({\r\n            map: this.map,\r\n            panel: \"panel\",\r\n          });\r\n\r\n          drivingOption.search(\r\n            // new AMap.LngLat(116.983518, 40.282746),\r\n            this.nowAddress,\r\n            new AMap.LngLat(zb.R, zb.Q),\r\n            function (status, result) {}\r\n          );\r\n\r\n          this.mouseTool.close(); //关闭\r\n        });\r\n      } else {\r\n        this.$message({\r\n          message: \"请先切换Eage浏览器确保定位成功\",\r\n          type: \"warning\",\r\n        });\r\n      }\r\n    },\r\n\r\n    onComplete(data) {\r\n      console.log(111, data);\r\n      // document.getElementById(\"status\").innerHTML = \"定位成功\";\r\n      // var str = [];\r\n      // str.push(\"定位结果：\" + data.position);\r\n      // str.push(\"定位类别：\" + data.location_type);\r\n      // if (data.accuracy) {\r\n      //   str.push(\"精度：\" + data.accuracy + \" 米\");\r\n      // } //如为IP精确定位结果则没有精度信息\r\n      // str.push(\"是否经过偏移：\" + (data.isConverted ? \"是\" : \"否\"));\r\n      // document.getElementById(\"result\").innerHTML = str.join(\"<br>\");\r\n    },\r\n\r\n    // 裁剪边界\r\n\r\n    // 打开卫星地图\r\n    addSatellite() {\r\n      // console.log(111);\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.satellite = new AMap.TileLayer.Satellite();\r\n      this.map.add(this.satellite);\r\n    },\r\n\r\n    // 关闭卫星地图\r\n    removeSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.map.remove(this.satellite);\r\n    },\r\n\r\n    // 打开路网\r\n    addRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.roadNetLayer = new AMap.TileLayer.RoadNet();\r\n      this.map.add(this.roadNetLayer);\r\n    },\r\n\r\n    // 关闭路网\r\n    removeRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.map.remove(this.roadNetLayer);\r\n    },\r\n    // 输入建议start\r\n    handleSelect(item) {\r\n      console.log(item);\r\n      this.searchObj = item;\r\n      console.log(this.searchIndustry);\r\n    },\r\n\r\n    querySearch(queryString, cb) {\r\n      var restaurants = this.restaurants;\r\n      var results = queryString\r\n        ? restaurants.filter(this.createFilter(queryString))\r\n        : restaurants;\r\n      // 调用 callback 返回建议列表的数据\r\n      cb(results);\r\n    },\r\n    createFilter(queryString) {\r\n      return (restaurants) => {\r\n        return (\r\n          restaurants.name.toLowerCase().indexOf(queryString.toLowerCase()) ===\r\n          0\r\n        );\r\n      };\r\n    },\r\n\r\n    // 复位事件\r\n    resetMap() {\r\n      location.reload();\r\n      // this.map.setCenter([115.464523, 38.874476]);\r\n    },\r\n\r\n    clearDrawGeometry() {\r\n      this.mouseTool.close(true);\r\n      this.geometrySearchRes = [];\r\n    },\r\n    closeDraw() {\r\n      this.mouseTool.close(true);\r\n      this.geometryBox = false;\r\n\r\n      this.geometrySearchRes = [];\r\n    },\r\n\r\n    changeissearchRes() {\r\n      this.issearchRes = !this.issearchRes;\r\n    },\r\n\r\n    // 类型查询end\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.heritage-industry #container {\r\n  border-bottom: 1px solid #f9d5a7;\r\n}\r\n/* 弹框样式 */\r\n.content-window-card {\r\n  position: relative;\r\n  box-shadow: none;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: auto;\r\n  padding: 0;\r\n}\r\n\r\n.content-window-card p {\r\n  height: 2rem;\r\n}\r\n\r\n.custom-info {\r\n  border: solid 1px silver;\r\n  width: 280px;\r\n  background-color: white;\r\n}\r\n\r\ndiv.info-top {\r\n  position: relative;\r\n  background: none repeat scroll 0 0 #f9f9f9;\r\n  border-bottom: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n}\r\n\r\ndiv.info-top div {\r\n  display: inline-block;\r\n  color: #333333;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  line-height: 31px;\r\n  padding: 0 10px;\r\n}\r\n\r\ndiv.info-top img {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  transition-duration: 0.25s;\r\n}\r\n\r\ndiv