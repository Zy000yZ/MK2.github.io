;;AACA,MAAA,MAAA,CAAA,MAAA;AAAA,4EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,OADA,GACA,MAAA,CAAA,GAAA,GAAA,KAAA,CAAA,MAAA,CAAA,MADA;AAAA;AAAA,yBAEA,MAAA,CAAA,MAAA,CAAA,IAAA,EACA;AACA,4FAFA,EAGA;AACA,oBAAA,OAAA,EAAA,OADA;AAEA,oBAAA,IAAA,EAAA,IAAA,CAAA;AAFA,mBAHA,CAFA;;AAAA;AAEA,kBAAA,GAFA;;AAWA,sBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA;AACA,sBAAA,OAAA,EAAA,QADA;AAEA,sBAAA,IAAA,EAAA;AAFA,qBAAA;;AAKA,oBAAA,MAAA,CAAA,QAAA,CAAA,YAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,mBAPA,MAOA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CACA,oCADA;AAGA;;AAtBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAwBA,KAtGA;AAwGA,IAAA,aAxGA,yBAwGA,CAxGA,EAwGA;AAAA;;AACA,UAAA,IAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AACA,MAAA,MAAA,CAAA,aAAA,CAAA,IAAA;;AACA,MAAA,MAAA,CAAA,MAAA;AAAA,4EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,OADA,GACA,MAAA,CAAA,GAAA,GAAA,KAAA,CAAA,MAAA,CAAA,MADA;AAAA;AAAA,yBAEA,MAAA,CAAA,MAAA,CAAA,IAAA,EACA;AACA,4FAFA,EAGA;AACA,oBAAA,OAAA,EAAA,OADA;AAEA,oBAAA,IAAA,EAAA,IAAA,CAAA;AAFA,mBAHA,CAFA;;AAAA;AAEA,kBAAA,GAFA;;AAWA,sBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA;AACA,sBAAA,OAAA,EAAA,QADA;AAEA,sBAAA,IAAA,EAAA;AAFA,qBAAA;;AAKA,oBAAA,MAAA,CAAA,QAAA,CAAA,aAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,mBAPA,MAOA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CACA,oCADA;AAGA;;AAtBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAwBA;AArIA;AAlJA,CAAA","sourcesContent":["<template>\r\n  <!--  -->\r\n  <div class=\"HeritageAdd\">\r\n    <div class=\"HeritageAdd-bottom padding15\">\r\n      <br />\r\n      <el-button @click=\"getCoordinateP\" v-show=\"newCoordinate.length === 0\"\r\n        >请先拾取流动摊贩地理坐标位置</el-button\r\n      >\r\n\r\n      <div class=\"main-box\" v-show=\"newCoordinate.length !== 0\">\r\n        <el-form\r\n          size=\"mini\"\r\n          :model=\"ruleForm\"\r\n          :rules=\"rules\"\r\n          ref=\"ruleForm\"\r\n          label-width=\"100px\"\r\n          class=\"demo-ruleForm\"\r\n        >\r\n          <el-form-item label=\"摊贩名称\" prop=\"name\">\r\n            <el-input v-model=\"ruleForm.name\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"摊贩类型\">\r\n            <el-select v-model=\"ruleForm.type\" placeholder=\"请选择\">\r\n              <el-option\r\n                v-for=\"item in options\"\r\n                :key=\"item.value\"\r\n                :label=\"item.label\"\r\n                :value=\"item.value\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"坐标经度\" prop=\"jing\">\r\n            <el-input style=\"width: 200px\" v-model=\"ruleForm.jing\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"坐标纬度\" prop=\"wei\">\r\n            <el-input style=\"width: 200px\" v-model=\"ruleForm.wei\"></el-input>\r\n            <el-button @click=\"getCoordinateP\" style=\"margin-left: 10px\"\r\n              >重新拾取</el-button\r\n            >\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"创建日期\" prop=\"date\" :disabled=\"true\">\r\n            <el-input v-model=\"ruleForm.date\"></el-input>\r\n          </el-form-item>\r\n          <!-- <el-form-item label=\"城市\">\r\n            <el-input\r\n              v-model=\"ruleForm.address.city\"\r\n              :disabled=\"true\"\r\n            ></el-input>\r\n          </el-form-item> -->\r\n\r\n          <el-form-item label=\"县区\">\r\n            <el-input\r\n              v-model=\"ruleForm.address.district\"\r\n              :disabled=\"true\"\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"乡镇\">\r\n            <el-input\r\n              v-model=\"ruleForm.address.township\"\r\n              :disabled=\"true\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"街道\">\r\n            <el-input\r\n              v-model=\"ruleForm.address.street\"\r\n              :disabled=\"true\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n        <div class=\"subBtn\">\r\n          <el-button type=\"primary\" @click=\"submitForm('ruleForm')\"\r\n            >立即创建</el-button\r\n          >\r\n          <el-button @click=\"resetForm('ruleForm')\">清空提示</el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"HeritageAdd-images\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"HeritageAdd\",\r\n\r\n  computed: {\r\n    ...mapState([\"lang\"]),\r\n    ...mapState([\"newCoordinate\", \"instryType\"]),\r\n  },\r\n  async created() {\r\n    const now = new Date();\r\n    const year = now.getFullYear();\r\n    const month = now.getMonth() + 1; // 月份从0开始，需要加1\r\n    const day = now.getDate();\r\n\r\n    console.log(year, month, day);\r\n    this.ruleForm.date = year + \"年\" + month + \"月\" + day + \"日\";\r\n    // 将坐标拾取状态改为add\r\n    console.log(this.newCoordinate);\r\n    this.changeCoordinateType(\"add\");\r\n    console.log(this.$route);\r\n    if (this.$route.params.editid !== \"add\") {\r\n      var res = \"\";\r\n\r\n      if (this.instryType === \"one\") {\r\n        res = await this.$axios.post(\"/LandJianKongGetOne/LandJianKongGetOne\", {\r\n          _id: this.$route.params.editid,\r\n        });\r\n      }\r\n\r\n      console.log(res.data.data);\r\n\r\n      this.ruleForm = res.data.data[0];\r\n      this.ruleForm.coordinate = this.ruleForm.coon.split(\",\");\r\n      this.ruleForm.jing = this.ruleForm.coordinate[0];\r\n      this.ruleForm.wei = this.ruleForm.coordinate[1];\r\n      console.log(\"编辑数据\", this.ruleForm);\r\n      this.getCoordinate(this.ruleForm.coordinate);\r\n    } else {\r\n      this.ruleForm.jing = this.newCoordinate[0];\r\n      this.ruleForm.wei = this.newCoordinate[1];\r\n    }\r\n    AMap.plugin(\"AMap.Geocoder\", () => {\r\n      var geocoder = new AMap.Geocoder({\r\n        city: \"010\",\r\n      });\r\n\r\n      var lnglat = [this.ruleForm.jing, this.ruleForm.wei];\r\n\r\n      geocoder.getAddress(lnglat, (status, result) => {\r\n        if (status === \"complete\" && result.info === \"OK\") {\r\n          // result为对应的地理位置详细信息\r\n\r\n          this.ruleForm.address.city = result.regeocode.addressComponent.city;\r\n          this.ruleForm.address.district =\r\n            result.regeocode.addressComponent.district;\r\n          this.ruleForm.address.township =\r\n            result.regeocode.addressComponent.township;\r\n          this.ruleForm.address.street =\r\n            result.regeocode.addressComponent.street;\r\n\r\n          console.log(111, this.ruleForm.address);\r\n        }\r\n      });\r\n    });\r\n  },\r\n  data() {\r\n    return {\r\n      ruleForm: {\r\n        name: \"\", //\r\n\r\n        type: \"熟食小吃\",\r\n        jing: 0,\r\n        wei: 0,\r\n        coon: \"\",\r\n        coordinate: [],\r\n\r\n        address: {\r\n          city: \"\",\r\n          district: \"\",\r\n          township: \"\",\r\n          street: \"\",\r\n        },\r\n        date: \"\",\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: \"请输入摊贩名称\", trigger: \"blur\" },\r\n          { min: 3, message: \"长度在不少于3个字符\", trigger: \"blur\" },\r\n        ],\r\n\r\n        date: [\r\n          {\r\n            required: true,\r\n            message: \"请输入日期\",\r\n            trigger: \"change\",\r\n          },\r\n        ],\r\n        jing: [\r\n          {\r\n            required: true,\r\n            message: \"请输入流动摊贩地理坐标\",\r\n            trigger: \"change\",\r\n          },\r\n        ],\r\n        wei: [\r\n          {\r\n            required: true,\r\n            message: \"请输入流动摊贩地理坐标\",\r\n            trigger: \"change\",\r\n          },\r\n        ],\r\n      },\r\n\r\n      options: [\r\n        {\r\n          value: \"熟食小吃\",\r\n          label: \"熟食小吃\",\r\n        },\r\n        {\r\n          value: \"蔬菜瓜果\",\r\n          label: \"蔬菜瓜果\",\r\n        },\r\n\r\n        {\r\n          value: \"水产肉类\",\r\n          label: \"水产肉类\",\r\n        },\r\n        {\r\n          value: \"精品饮料\",\r\n          label: \"精品饮料\",\r\n        },\r\n        {\r\n          value: \"宠物销售\",\r\n          label: \"宠物销售\",\r\n        },\r\n        {\r\n          value: \"缝补维修\",\r\n          label: \"缝补维修\",\r\n        },\r\n        {\r\n          value: \"其他\",\r\n          label: \"其他\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    ...mapMutations([\r\n      \"getCoordinate\",\r\n      \"changeCoordinateType\",\r\n      \"getapplyCoordinate\",\r\n    ]),\r\n\r\n    //跳转页面拾取坐标位置\r\n    getCoordinateP() {\r\n      this.getapplyCoordinate([]);\r\n      this.$router.push(\"/get/coordinate\");\r\n    },\r\n\r\n    submitForm(formName) {\r\n      console.log(formName);\r\n      console.log(this.ruleForm);\r\n      this.$refs[formName].validate(async (valid) => {\r\n        if (valid) {\r\n          // 格式化数据\r\n          this.ruleForm.coordinate = [this.ruleForm.jing, this.ruleForm.wei];\r\n          this.ruleForm.coon = this.ruleForm.jing + `,` + this.ruleForm.wei;\r\n          // 格式已经处理完毕，下面发送请求\r\n          console.log(this.ruleForm);\r\n\r\n          // 发送请求;\r\n          if (this.$route.params.editid !== \"add\") {\r\n            // 编辑部分\r\n            var res = await this.$axios.post(\r\n              \"/editHeritageMainData/editHeritageMainData\",\r\n              {\r\n                _id: this.ruleForm._id,\r\n                addType: this.ruleForm.addType,\r\n                name: this.ruleForm.name,\r\n                company: this.ruleForm.company,\r\n                start: this.ruleForm.start,\r\n                prolevel: this.ruleForm.prolevel,\r\n                coon: this.ruleForm.coon,\r\n                brief: this.ruleForm.brief,\r\n              }\r\n            );\r\n            console.log(\"res====\", res);\r\n            if (res.status === 200) {\r\n              this.$message({\r\n                message: \"信息编辑成功\",\r\n                type: \"success\",\r\n              });\r\n              this.getCoordinate([]); //将拾取到的坐标清空\r\n              this.$router.push(\"/data/manage\");\r\n            }\r\n          } else {\r\n              console.log(this.ruleForm);\r\n              var res2 = await this.$axios.post(\r\n                \"/ShiChangBoothAddData/ShiChangBoothAddData\",\r\n                this.ruleForm\r\n              );\r\n              if (res2.status === 200) {\r\n                this.$notify({\r\n                  title: \"流动摊贩信息添加成功\",\r\n                });\r\n                this.getCoordinate([]); //将拾取到的坐标清空\r\n                this.$router.push(\"/data/manage\");\r\n              }\r\n          }\r\n        } else {\r\n          this.$message.error(\"请检查完善相关信息\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n\r\n    addImagesAll(e) {\r\n      const file = e.target.files[0];\r\n      console.log(file);\r\n      let reader = new FileReader();\r\n      reader.readAsDataURL(file);\r\n      reader.onload = async (event) => {\r\n        let dataUrl = (this.src = event.target.result);\r\n        var res = await this.$axios.post(\r\n          // \"https://790d5b85-9674-4a89-9bcc-c0657ea369be.bspapp.com/image\",\r\n          \"https://fc-mp-790d5b85-9674-4a89-9bcc-c0657ea369be.next.bspapp.com/image\",\r\n          {\r\n            dataUrl,\r\n            name: file.name,\r\n          }\r\n        );\r\n\r\n        if (res.status === 200) {\r\n          this.$message({\r\n            message: \"图片上传成功\",\r\n            type: \"success\",\r\n          });\r\n\r\n          this.ruleForm.imagesAllurl.push(res.data.fileID);\r\n        } else {\r\n          this.$message.error(\r\n            \"图片上传失败，请选择PNG、jpg等图片格式并确定图片大小小于1MB\"\r\n          );\r\n        }\r\n      };\r\n    },\r\n\r\n    addImagesAllC(e) {\r\n      const file = e.target.files[0];\r\n      console.log(file);\r\n      let reader = new FileReader();\r\n      reader.readAsDataURL(file);\r\n      reader.onload = async (event) => {\r\n        let dataUrl = (this.src = event.target.result);\r\n        var res = await this.$axios.post(\r\n          // \"https://790d5b85-9674-4a89-9bcc-c0657ea369be.bspapp.com/image\",\r\n          \"https://fc-mp-790d5b85-9674-4a89-9bcc-c0657ea369be.next.bspapp.com/image\",\r\n          {\r\n            dataUrl,\r\n            name: file.name,\r\n          }\r\n        );\r\n\r\n        if (res.status === 200) {\r\n          this.$message({\r\n            message: \"图片上传成功\",\r\n            type: \"success\",\r\n          });\r\n\r\n          this.ruleForm.imagesAllurlC.push(res.data.fileID);\r\n        } else {\r\n          this.$message.error(\r\n            \"图片上传失败，请选择PNG、jpg等图片格式并确定图片大小小于1MB\"\r\n          );\r\n        }\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.HeritageAdd-bottom {\r\n  background-color: white;\r\n  margin: 20px;\r\n}\r\n/* .HeritageAdd {\r\n  overflow: auto;\r\n} */\r\n</style>\r\n"],"sourceRoot":"src/views/Heritage"}]}