DA;AAJA;AARA,KAAA;AAiBA,GAxBA;AA0BA,EAAA,OA1BA,qBA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA;AADA;AAAA,qBAEA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,sCAAA,EAAA;AACA,gBAAA,GAAA,EAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA;AADA,eAAA,CAFA;;AAAA;AAEA,cAAA,GAFA;AAKA,cAAA,KAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,gBAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,GAjCA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,QADA,EACA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA;AAAA,2EAAA,kBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KADA;AAAA;AAAA;AAAA;;AAEA,kBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,QAAA;;AACA,sBAAA,MAAA,CAAA,QAAA,CAAA,cAAA,KAAA,IAAA,EAAA;AACA,oBAAA,MAAA,CAAA,gBAAA,CAAA,cAAA,GAAA,OAAA;AACA,mBAFA,MAEA,IAAA,MAAA,CAAA,QAAA,CAAA,cAAA,KAAA,IAAA,EAAA;AACA,oBAAA,MAAA,CAAA,gBAAA,CAAA,cAAA,GAAA,QAAA;AACA;;AAEA,kBAAA,MAAA,CAAA,gBAAA,CAAA,gBAAA,GACA,MAAA,CAAA,QAAA,CAAA,gBADA;AAEA,kBAAA,MAAA,CAAA,gBAAA,CAAA,aAAA,GAAA,MAAA,CAAA,QAAA,CAAA,aAAA;AAXA;AAAA,yBAYA,MAAA,CAAA,MAAA,CAAA,IAAA,CACA,kCADA,EAEA,MAAA,CAAA,gBAFA,CAZA;;AAAA;AAYA,kBAAA,GAZA;;AAiBA,sBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA;AACA,sBAAA,OAAA,EAAA,MADA;AAEA,sBAAA,IAAA,EAAA;AAFA,qBAAA;;AAIA,oBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,iBAAA;AACA;;AAvBA;AAAA;;AAAA;AAyBA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,SAAA;;AAzBA,oDA0BA,KA1BA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AA6BA;AA/BA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <!-- 工业遗产报修审批的详情 -->\r\n  <div class=\"ApplyHeritageMain\">\r\n    <div class=\"ApplyHeritageMain-top\">\r\n      <MainTop>遗产数据 <span class=\"gang\"> / </span> 报修详情</MainTop>\r\n    </div>\r\n    <div class=\"ApplyHeritageMain-bottom padding15\">\r\n      <el-alert\r\n        title=\"\r\n        \r\n            管理员，欢迎您,请对用户的报修申请进行客观审批。\r\n           \r\n        \"\r\n        type=\"info\"\r\n      >\r\n      </el-alert>\r\n      <br />\r\n      <el-card v-if=\"heritageMainData !== null\" class=\"box-card\" shadow=\"hover\">\r\n        <div slot=\"header\" class=\"clearfix\">\r\n          <span style=\"color: black\" class=\"title\">{{\r\n            heritageMainData.name\r\n          }}</span>\r\n          <div>\r\n            <span style=\"margin-right: 10px\">{{\r\n              heritageMainData.addType\r\n            }}</span>\r\n\r\n            <el-tag\r\n              v-if=\"heritageMainData.approvalStatus === 'pending'\"\r\n              type=\"info\"\r\n              >待审批</el-tag\r\n            >\r\n            <el-tag\r\n              v-if=\"heritageMainData.approvalStatus === 'adopt'\"\r\n              type=\"warning\"\r\n              >处理中</el-tag\r\n            >\r\n            <el-tag\r\n              v-if=\"heritageMainData.approvalStatus === 'reject'\"\r\n              type=\"danger\"\r\n              >已驳回</el-tag\r\n            >\r\n            <el-tag\r\n              v-if=\"heritageMainData.approvalStatus === 'isend'\"\r\n              type=\"success\"\r\n              >已完成</el-tag\r\n            >\r\n          </div>\r\n        </div>\r\n        <div class=\"text item\">\r\n          <div>\r\n            <div class=\"text-che\">\r\n              <span>报修人：{{ heritageMainData.applyName }}</span>\r\n            </div>\r\n            <div class=\"text-che\">\r\n              <span>联系方式：{{ heritageMainData.phone }}</span>\r\n            </div>\r\n            <div class=\"text-che\">\r\n              <span>报修类型：{{ heritageMainData.repairType }}</span>\r\n            </div>\r\n            <div class=\"text-che\">\r\n              <span>报修详情：</span>\r\n            </div>\r\n            <div class=\"text-che\" v-html=\"heritageMainData.repairMain\"></div>\r\n            <div class=\"text-che\">\r\n              <span>是否自备材料及配件：{{ heritageMainData.isPart }}</span>\r\n            </div>\r\n\r\n            <div class=\"text-che\" v-if=\"heritageMainData.partMain !== ''\">\r\n              <span>材料描述：</span>\r\n            </div>\r\n            <div\r\n              class=\"text-che\"\r\n              v-if=\"heritageMainData.partMain !== ''\"\r\n              v-html=\"heritageMainData.partMain\"\r\n            ></div>\r\n\r\n            <el-divider></el-divider>\r\n            <div class=\"text-che\">\r\n              <span>始建年份：{{ heritageMainData.start }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"text-che\">\r\n            <span>遗产类型：{{ heritageMainData.type }}</span>\r\n          </div>\r\n          <div class=\"text-che\">\r\n            <span>遗产地址：{{ heritageMainData.address }}</span>\r\n          </div>\r\n          <div class=\"text-che\">\r\n            <span>所属公司：{{ heritageMainData.company }}</span>\r\n          </div>\r\n          <div class=\"text-che\">\r\n            <span\r\n              >地理坐标：{{ heritageMainData.jing }},{{\r\n                heritageMainData.wei\r\n              }}</span\r\n            >\r\n          </div>\r\n          <div class=\"text-che\">\r\n            <span> 保护等级及再利用情况：</span>\r\n            <p>\r\n              {{\r\n                heritageMainData.prolevel\r\n                  ? heritageMainData.prolevel\r\n                  : \"暂无数据\"\r\n              }}\r\n            </p>\r\n          </div>\r\n\r\n          <el-divider></el-divider>\r\n          <div class=\"text-che\">\r\n            <span>遗产预览： </span>\r\n          </div>\r\n\r\n          <!-- 首页图片 -->\r\n          <div class=\"text-che\">\r\n            <img class=\"mainImage\" :src=\"heritageMainData.mainImage\" alt=\"\" />\r\n          </div>\r\n          <el-divider></el-divider>\r\n\r\n          <div class=\"text-che\">\r\n            <span>相关图片 </span>\r\n          </div>\r\n\r\n          <div class=\"text-che\">\r\n            <el-carousel :interval=\"4000\" type=\"card\" height=\"500px\">\r\n              <el-carousel-item\r\n                v-for=\"item in heritageMainData.imagesAllurl\"\r\n                :key=\"item\"\r\n              >\r\n                <img class=\"otherImage\" :src=\"item\" alt=\"\" />\r\n              </el-carousel-item>\r\n            </el-carousel>\r\n          </div>\r\n\r\n          <div class=\"text-che\">\r\n            <span>审批意见： </span>\r\n          </div>\r\n          <div\r\n            v-if=\"heritageMainData.approvalComments !== ''\"\r\n            class=\"text-che\"\r\n            v-html=\"heritageMainData.approvalComments\"\r\n          ></div>\r\n          <div v-else class=\"text-che\"><p>暂无意见</p></div>\r\n          <el-divider></el-divider>\r\n\r\n          <!-- 审批表单部分 -->\r\n          <el-form\r\n            :model=\"ruleForm\"\r\n            :rules=\"rules\"\r\n            ref=\"ruleForm\"\r\n            label-width=\"100px\"\r\n            class=\"demo-ruleForm\"\r\n          >\r\n            <el-form-item label=\"审批结果\" prop=\"approvalStatus\">\r\n              <el-radio-group v-model=\"ruleForm.approvalStatus\">\r\n                <el-radio label=\"同意\" value=\"adopt\"></el-radio>\r\n                <el-radio label=\"驳回\" value=\"reject\"></el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"审批意见\" prop=\"approvalComments\">\r\n              <el-input\r\n                type=\"textarea\"\r\n                v-model=\"ruleForm.approvalComments\"\r\n              ></el-input>\r\n            </el-form-item>\r\n\r\n            <el-form-item\r\n              v-if=\"ruleForm.approvalStatus === '驳回'\"\r\n              label=\"驳回原因\"\r\n            >\r\n              <el-radio-group v-model=\"ruleForm.rejectComment\">\r\n                <el-radio label=\"图片不全\"></el-radio>\r\n                <el-radio label=\"无需派工维修\"></el-radio>\r\n                <el-radio label=\"遗产报修相关内容不完善\"></el-radio>\r\n                <el-radio label=\"其他\"></el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submitForm('ruleForm')\"\r\n                >立即创建</el-button\r\n              >\r\n            </el-form-item>\r\n          </el-form>\r\n          <!-- end -->\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MainTop from \"../../components/MainTop.vue\";\r\n\r\nexport default {\r\n  name: \"ApplyHeritageMain\",\r\n  components: {\r\n    MainTop,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      heritageMainData: null,\r\n      ruleForm: {\r\n        approvalStatus: \"\",\r\n        approvalComments: \"\",\r\n        rejectComment: \"\",\r\n      },\r\n\r\n      rules: {\r\n        approvalStatus: [\r\n          { required: true, message: \"请输入选择审批类型\", trigger: \"blur\" },\r\n        ],\r\n        approvalComments: [\r\n          { required: true, message: \"请输入审批意见\", trigger: \"change\" },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    console.log(this.$route);\r\n    var res = await this.$axios.post(\"/getOneApplyRepair/getOneApplyRepair\", {\r\n      _id: this.$route.query._id,\r\n    });\r\n    this.heritageMainData = res.data.data[0];\r\n    console.log(this.heritageMainData);\r\n  },\r\n\r\n  methods: {\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate(async (valid) => {\r\n        if (valid) {\r\n          console.log(this.ruleForm);\r\n          if (this.ruleForm.approvalStatus === \"同意\") {\r\n            this.heritageMainData.approvalStatus = \"adopt\";\r\n          } else if (this.ruleForm.approvalStatus === \"驳回\") {\r\n            this.heritageMainData.approvalStatus = \"reject\";\r\n          }\r\n\r\n          this.heritageMainData.approvalComments =\r\n            this.ruleForm.approvalComments;\r\n          this.heritageMainData.rejectComment = this.ruleForm.rejectComment;\r\n          var res = await this.$axios.post(\r\n            \"/editApplyRepair/editApplyRepair\",\r\n            this.heritageMainData\r\n          );\r\n\r\n          if (res.status === 200) {\r\n            this.$message({\r\n              message: \"审批成功\",\r\n              type: \"success\",\r\n            });\r\n            this.$router.push(\"/repair/approve\");\r\n          }\r\n        } else {\r\n          this.$message.error(\"请完善审批信息\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style >\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .clearfix {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .clearfix span {\r\n  font-size: 16px;\r\n}\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .clearfix .title {\r\n  font-size: 20px;\r\n}\r\n\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .text span {\r\n  font-size: 15px;\r\n}\r\n\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .text .text-che {\r\n  padding: 5px 0;\r\n}\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .text p {\r\n  margin: 0;\r\n  margin-left: 5px;\r\n  color: black;\r\n  font-family: \"楷体\";\r\n  padding: 8px 0;\r\n  text-indent: 35px;\r\n  word-break: break-all;\r\n}\r\n.ApplyHeritageMain .ApplyHeritageMain-bottom .text .mainImage {\r\n  width: 80%;\r\n  height: auto;\r\n  display: block;\r\n  margin: 0 auto;\r\n}\r\n/* .ApplyHeritageMain .ApplyHeritageMain-bottom .text .otherImage {\r\n\r\n} */\r\n</style>"],"sourceRoot":"src/views/Heritage"}]}