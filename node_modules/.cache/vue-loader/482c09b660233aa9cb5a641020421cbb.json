CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"LanPointMap.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"heritage-industry\">\r\n    <!-- 地图容器 -->\r\n    <div class=\"map-buttons\">\r\n      <!-- 类型搜索start -->\r\n      <!-- <el-select\r\n        class=\"typeSelect\"\r\n        v-model=\"typeSelect\"\r\n        placeholder=\"请选择类型\"\r\n        size=\"mini\"\r\n      >\r\n        <el-option\r\n          v-for=\"item in options\"\r\n          :key=\"item.name\"\r\n          :label=\"item.name\"\r\n          :value=\"item.name\"\r\n        >\r\n        </el-option>\r\n      </el-select>\r\n      <el-button\r\n        id=\"selectBtn\"\r\n        class=\"selectBtn\"\r\n        size=\"mini\"\r\n        icon=\"el-icon-search\"\r\n        @click=\"searchTypeClick\"\r\n      ></el-button> -->\r\n      <el-select\r\n        size=\"mini\"\r\n        @change=\"changeDistrict\"\r\n        v-model=\"currentQu.qu\"\r\n        placeholder=\"请选择\"\r\n        style=\"width: 150px; margin-right: 15px\"\r\n      >\r\n        <el-option\r\n          v-for=\"item in cityCode\"\r\n          :key=\"item.qu\"\r\n          :label=\"item.qu\"\r\n          :value=\"item.qu\"\r\n        >\r\n        </el-option>\r\n      </el-select>\r\n      <el-select\r\n        size=\"mini\"\r\n        @change=\"changeTownship\"\r\n        v-model=\"currentQu.township\"\r\n        placeholder=\"请选择\"\r\n        style=\"width: 150px\"\r\n      >\r\n        <el-option\r\n          v-for=\"item in districtList\"\r\n          :key=\"item.name\"\r\n          :label=\"item.name\"\r\n          :value=\"item.name\"\r\n        >\r\n        </el-option>\r\n      </el-select>\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addRoadLayer\"\r\n        v-show=\"!roadNetwork\"\r\n        class=\"map-btn map-element\"\r\n        style=\"margin-left: 15px\"\r\n        >{{ lang === \"zh_cn\" ? \"打开路网\" : \"Open road\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"removeRoadLayer\"\r\n        v-show=\"roadNetwork\"\r\n        class=\"map-btn map-element\"\r\n        style=\"margin-left: 15px\"\r\n        >{{ lang === \"zh_cn\" ? \"关闭路网\" : \"Close road\" }}</el-button\r\n      >\r\n\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addSatellite\"\r\n        v-show=\"isOrdMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"卫星地图\" : \"Satellite map\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"removeSatellite\"\r\n        v-show=\"!isOrdMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"普通地图\" : \"General map\" }}</el-button\r\n      >\r\n\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"resetMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"刷新\" : \"Reset\" }}</el-button\r\n      >\r\n    </div>\r\n    <div id=\"container\" class=\"container\">\r\n      <div id=\"panel\"></div>\r\n      <div class=\"egColor\">\r\n        <p>图例</p>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/电杆0808.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>电杆</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/人井0808.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>人井</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"egBox\">\r\n        <p>停车场信息</p>\r\n        <div class=\"egColor-che\">\r\n          <span>停车场名称： {{ egBoxData.name }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>停车场面积（平方米）： {{ egBoxData.mianji }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>停车场负责人： {{ egBoxData.parkadmin }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>联系方式： {{ egBoxData.parktel }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>车位数： {{ egBoxData.parknum }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>收费标准： {{ egBoxData.parkmoney }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>数据更新时间： {{ egBoxData.adddate }}</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\nexport default {\r\n  name: \"HeritageIndustry\",\r\n\r\n  data() {\r\n    return {\r\n      map: \"\",\r\n      district: null,\r\n      polygons: [],\r\n      zoom: 12,\r\n      polygon: [], //点位\r\n\r\n      // 弹框内容\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n\r\n      dataList: null, // 数据\r\n      mainData: null,\r\n      isOrdMap: true, //判断是卫星图还是普通图，普通图为true\r\n      satellite: null, //用来存储卫星地图影像\r\n      roadNetwork: false, //是否具有路网，默认没有\r\n      roadNetLayer: null, //用来存储路网\r\n\r\n      searchIndustry: \"\",\r\n      markers: [],\r\n\r\n      egBoxData: {\r\n        name: \"暂无数据\",\r\n        mianji: \"暂无数据\",\r\n        path: \"暂无数据\",\r\n        type: \"暂无数据\",\r\n      },\r\n\r\n      typeSelect: \"所有停车场\",\r\n\r\n      options: [\r\n        {\r\n          name: \"所有停车场\",\r\n        },\r\n      ],\r\n\r\n      cityCode: [\r\n        { id: 1, city: \"上海市\", qu: \"黄浦区\", code: \"01\" },\r\n        { id: 2, city: \"上海市\", qu: \"徐汇区\", code: \"02\" },\r\n        { id: 3, city: \"上海市\", qu: \"长宁区\", code: \"03\" },\r\n        { id: 4, city: \"上海市\", qu: \"静安区\", code: \"04\" },\r\n        { id: 5, city: \"上海市\", qu: \"普陀区\", code: \"05\" },\r\n        { id: 6, city: \"上海市\", qu: \"虹口区\", code: \"06\" },\r\n        { id: 7, city: \"上海市\", qu: \"杨浦区\", code: \"07\" },\r\n        { id: 8, city: \"上海市\", qu: \"闵行区\", code: \"08\" },\r\n        { id: 9, city: \"上海市\", qu: \"宝山区\", code: \"09\" },\r\n        { id: 10, city: \"上海市\", qu: \"嘉定区\", code: \"10\" },\r\n        { id: 11, city: \"上海市\", qu: \"浦东新区\", code: \"11\" },\r\n        { id: 12, city: \"上海市\", qu: \"金山区\", code: \"12\" },\r\n        { id: 13, city: \"上海市\", qu: \"松江区\", code: \"13\" },\r\n        { id: 14, city: \"上海市\", qu: \"青浦区\", code: \"14\" },\r\n        { id: 15, city: \"上海市\", qu: \"奉贤区\", code: \"15\" },\r\n        { id: 16, city: \"上海市\", qu: \"崇明区\", code: \"16\" },\r\n      ],\r\n      currentQu: {\r\n        qu: \"黄浦区\",\r\n        township: \"全部街道\",\r\n      },\r\n      districtList: [],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapState([\"lang\", \"nowAddress\", \"districtObj\"]),\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap(); //初始化地图\r\n    this.mouseTool = new AMap.MouseTool(this.map);\r\n    this.getDistrictList();\r\n    this.getQuPointData();\r\n  },\r\n\r\n  methods: {\r\n    searchTypeClick() {\r\n      this.map.clearMap();\r\n      this.polygons = [];\r\n\r\n      var newArray = [];\r\n      if (this.typeSelect === \"所有停车场\") {\r\n        newArray = this.dataList;\r\n      } else {\r\n        newArray = this.dataList.filter((ele) => ele.name === this.typeSelect);\r\n      }\r\n      console.log(newArray);\r\n\r\n      for (let i = 0; i < newArray.length; i++) {\r\n        var path = newArray[i].path.map((ele) => [ele.R, ele.Q]);\r\n\r\n        var colorType = {\r\n          fillColor: \"#1976D2\", // 多边形填充颜色\r\n          borderWeight: 2, // 线条宽度，默认为 1\r\n          strokeColor: \"#0D47A1\", // 线条颜色林地，绿地，湿地\r\n          strokeWeight: 6,\r\n          strokeOpacity: 0.2,\r\n          fillOpacity: 0.8,\r\n          strokeStyle: \"solid\",\r\n        };\r\n\r\n        let polygon = new AMap.Polygon({\r\n          path: path,\r\n          ...colorType,\r\n        });\r\n\r\n        polygon.on(\"click\", () => {\r\n          console.log(\"tets--\", newArray[i]);\r\n          this.egBoxData = {\r\n            name: newArray[i].name,\r\n            mianji: Math.floor(newArray[i].mianji / 10),\r\n            path: newArray[i].path.length,\r\n            adddate: newArray[i].adddate,\r\n            parkadmin: newArray[i].parkadmin,\r\n            parktel: newArray[i].parktel,\r\n            parkmoney: newArray[i].parkmoney,\r\n            parknum: newArray[i].parknum,\r\n          };\r\n        });\r\n        this.polygons.push(polygon);\r\n      }\r\n\r\n      this.map.add(this.polygons);\r\n      this.map.setFitView();\r\n    },\r\n\r\n    ...mapMutations([\"getNowAddress\"]),\r\n\r\n    initMap() {\r\n      this.map = new AMap.Map(\"container\", {\r\n        resizeEnable: true,\r\n        zoom: 14, //级别\r\n        zooms: [3, 30],\r\n        center: [121.47, 31.23], //中心点坐标\r\n        viewMode: \"3D\", //使用3D视图\r\n        lang: this.lang,\r\n        mapStyle: \"amap://styles/macaron\",\r\n        infoWindow: null,\r\n      });\r\n    },\r\n\r\n    getDistrictList() {\r\n      var districtArr = this.districtObj.districtList.find(\r\n        (ele) => ele.name === this.currentQu.qu\r\n      );\r\n      console.log(\"当前所有街道\", districtArr);\r\n      this.districtList = [{ name: \"全部街道\" }, ...districtArr.districtList];\r\n    },\r\n    // 获取点数据\r\n    async getQuPointData() {\r\n      var qucode = this.cityCode.find((ele) => ele.qu === this.currentQu.qu);\r\n      var res = null;\r\n      var str = \"/LanPoint\" + qucode.code + \"Get\";\r\n      res = await this.$axios.get(str + str);\r\n      if (res.status === 200) {\r\n        console.log(\"success\", this.currentQu.qu, res);\r\n        this.dataList = res.data.data;\r\n        this.restaurant = res.data.data;\r\n        this.addMarker();\r\n      }\r\n    },\r\n    changeDistrict() {\r\n      this.getDistrictList();\r\n      this.getQuPointData();\r\n      this.currentQu.township = \"全部街道\";\r\n    },\r\n    changeTownship() {\r\n      if (this.currentQu.township !== \"全部街道\") {\r\n        this.dataList = this.restaurant;\r\n        var newtableData = this.restaurant.filter(\r\n          (ele) => ele.addressObject.township === this.currentQu.township\r\n        );\r\n        this.dataList = newtableData;\r\n      } else {\r\n        this.dataList = this.restaurant;\r\n      }\r\n      this.addMarker()\r\n    },\r\n\r\n    // 添加点集\r\n    addMarker() {\r\n      this.map.clearMap();\r\n      this.markers=[]\r\n      for (let i = 0; i < this.dataList.length; i++) {\r\n        // 注意用 let\r\n        var url =\r\n          \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/电杆0808.png\";\r\n        if (this.dataList[i].property === \"人井\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/人井0808.png\";\r\n        }\r\n\r\n        let marker = new AMap.Marker({\r\n          icon: new AMap.Icon({\r\n            image: url,\r\n            size: new AMap.Size(30, 30),\r\n            imageSize: new AMap.Size(30, 30),\r\n          }),\r\n          map: this.map,\r\n          position: [this.dataList[i].jing, this.dataList[i].wei],\r\n          extData: this.dataList[i],\r\n        });\r\n        this.markers.push(marker);\r\n        AMap.event.addListener(marker, \"click\", () => {\r\n          // console.log('图标被点击了')\r\n          this.title = this.dataList[i].markName;\r\n          (this.content = [\r\n            \"安装地址：\" + this.dataList[i].address,\r\n            \"更新时间：\" + this.dataList[i].editData,\r\n            \"产权单位：\" + this.dataList[i].company,\r\n            \"负责人：\" + this.dataList[i].admin,\r\n          ]),\r\n            (this.infoWindow = new AMap.InfoWindow({\r\n              isCustom: true, //使用自定义窗体\r\n              content: this.createInfoWindow(\r\n                this.title,\r\n                this.content.join(\"<br/>\")\r\n              ),\r\n              offset: new AMap.Pixel(16, -45),\r\n            }));\r\n          this.infoWindow.open(this.map, marker.getPosition());\r\n        });\r\n      }\r\n      this.map.setFitView()\r\n    },\r\n\r\n    //渲染弹框\r\n    createInfoWindow(title, content) {\r\n      var info = document.createElement(\"div\");\r\n      info.className = \"custom-info input-card content-window-card\";\r\n      //可以通过下面的方式修改自定义窗体的宽高\r\n      //info.style.width = \"400px\";\r\n      // 定义顶部标题\r\n      var top = document.createElement(\"div\");\r\n      var titleD = document.createElement(\"div\");\r\n      var closeX = document.createElement(\"img\");\r\n      top.className = \"info-top\";\r\n      titleD.innerHTML = title;\r\n      closeX.src = \"https://webapi.amap.com/images/close2.gif\";\r\n      closeX.onclick = this.closeInfoWindow;\r\n\r\n      top.appendChild(titleD);\r\n      top.appendChild(closeX);\r\n      info.appendChild(top);\r\n\r\n      // 定义中部内容\r\n      var middle = document.createElement(\"div\");\r\n      middle.className = \"info-middle\";\r\n      middle.style.backgroundColor = \"white\";\r\n      middle.innerHTML = content;\r\n      info.appendChild(middle);\r\n\r\n      // 定义底部内容\r\n      var bottom = document.createElement(\"div\");\r\n      bottom.className = \"info-bottom\";\r\n      bottom.style.position = \"relative\";\r\n      bottom.style.top = \"0px\";\r\n      bottom.style.margin = \"0 auto\";\r\n      var sharp = document.createElement(\"img\");\r\n      sharp.src = \"https://webapi.amap.com/images/sharp.png\";\r\n      bottom.appendChild(sharp);\r\n      info.appendChild(bottom);\r\n      return info;\r\n    },\r\n\r\n    // 关闭弹框\r\n    closeInfoWindow() {\r\n      this.map.clearInfoWindow();\r\n      this.searchIndustry = null;\r\n    },\r\n    // 打开卫星地图\r\n    addSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.satellite = new AMap.TileLayer.Satellite();\r\n      this.map.add(this.satellite);\r\n    },\r\n\r\n    // 关闭卫星地图\r\n    removeSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.map.remove(this.satellite);\r\n    },\r\n\r\n    // 打开路网\r\n    addRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.roadNetLayer = new AMap.TileLayer.RoadNet();\r\n      this.map.add(this.roadNetLayer);\r\n    },\r\n\r\n    // 关闭路网\r\n    removeRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.map.remove(this.roadNetLayer);\r\n    },\r\n\r\n    // 复位事件\r\n    resetMap() {\r\n      location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.egBox {\r\n  background-color: white;\r\n  position: fixed;\r\n  top: 155px;\r\n  left: 225px;\r\n  z-index: 99;\r\n\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n  padding: 15px;\r\n}\r\n.egColor {\r\n  /* width: 300px;\r\n  height: 400px; */\r\n  background-color: white;\r\n  position: fixed;\r\n  top: 155px;\r\n  right: 30px;\r\n  z-index: 99;\r\n\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n  padding: 15px;\r\n}\r\n\r\n.egColor .egColor-che {\r\n  display: flex;\r\n  margin-top: 10px;\r\n  align-items: ce