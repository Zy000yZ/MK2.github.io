", \"markData2\"]),\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      table2Data: [],\r\n      markData: [],\r\n      dialogTableVisible: false,\r\n      radio1: \"土壤评估\",\r\n\r\n      mapType: \"normal\",\r\n      map: \"\",\r\n      marker: null, //点位\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n      markers: [],\r\n      satellite: null,\r\n\r\n      isPG: false,\r\n      scelOption: {\r\n        scl1: \"单因子法\",\r\n        scl2: \"整体评价\",\r\n        scl3: \"铜\",\r\n      },\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap();\r\n    this.tableChange();\r\n  },\r\n\r\n  methods: {\r\n    tableChange() {\r\n        this.isPG = false;\r\n      if (this.radio1 === \"土壤评估\") {\r\n        this.tableData = this.tableData1;\r\n      } else {\r\n        this.tableData = this.tableData2;\r\n      }\r\n\r\n      this.table2Change();\r\n      this.addMarker();\r\n    },\r\n    table2Change() {\r\n      this.table2Data = this.tableData.map((ele) => {\r\n        var x = ele.x2;\r\n\r\n        if (this.scelOption.scl3 === \"铬\") x = ele.x3;\r\n        if (this.scelOption.scl3 === \"镍\") x = ele.x4;\r\n        if (this.scelOption.scl3 === \"锌\") x = ele.x5;\r\n        if (this.scelOption.scl3 === \"铅\") x = ele.x6;\r\n        if (this.scelOption.scl3 === \"镉\") x = ele.x7;\r\n        if (this.scelOption.scl3 === \"砷\") x = ele.x8;\r\n        if (this.scelOption.scl3 === \"汞\") x = ele.x9;\r\n\r\n        return {\r\n          id: ele.id,\r\n          ph: ele.x1,\r\n          yname: this.scelOption.scl3,\r\n          x,\r\n        };\r\n      });\r\n    },\r\n\r\n    mapChange() {\r\n      if (this.mapType === \"yg\") {\r\n        this.satellite = new AMap.TileLayer.Satellite();\r\n        this.map.add(this.satellite);\r\n      } else {\r\n        if (this.satellite) {\r\n          this.map.remove(this.satellite);\r\n        }\r\n        var styleName = \"amap://styles/\" + this.mapType;\r\n        this.map.setMapStyle(styleName);\r\n      }\r\n    },\r\n\r\n    pgclick() {\r\n      this.isPG = true;\r\n        this.$message({\r\n              message: \"评估成功\",\r\n              type: \"success\",\r\n            });\r\n      this.addMarker();\r\n    },\r\n\r\n    initMap() {\r\n      //初始化地图\r\n      this.map = new AMap.Map(\"container0307\", {\r\n        resizeEnable: true,\r\n        zoom: 12, //级别\r\n        zooms: [6.5, 30],\r\n        center: [100.13, 26.48], //中心点坐标\r\n        viewMode: \"2D\", //使用3D视图\r\n        mapStyle: \"amap://styles/normal\",\r\n        infoWindow: null,\r\n      });\r\n    },\r\n\r\n    // 添加点坐标\r\n    addMarker() {\r\n      if (this.markers.length !== 0) {\r\n        this.map.remove(this.markers);\r\n        this.map.clearInfoWindow();\r\n        this.markers = [];\r\n      }\r\n\r\n      if (this.radio1 === \"土壤评估\") {\r\n        this.markData = this.markData1;\r\n      } else {\r\n        this.markData = this.markData2;\r\n      }\r\n      for (let i = 0; i < this.markData.length; i++) {\r\n        // 注意这里一定得用 let\r\n        var iconUrl =\r\n          \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/点0310.png\";\r\n\r\n        if (this.isPG) {\r\n          if (this.markData[i].color === 0)\r\n            iconUrl =\r\n              \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/点 (1)0310.png\";\r\n          if (this.markData[i].color === 1)\r\n            iconUrl =\r\n              \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/点 (2)0310.png\";\r\n          if (this.markData[i].color === 2)\r\n            iconUrl =\r\n              \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/点 (3)0310.png\";\r\n        }\r\n        let marker = new AMap.Marker({\r\n          // 自定义图标\r\n          icon: new AMap.Icon({\r\n            image: iconUrl,\r\n            size: new AMap.Size(20, 20),\r\n            imageSize: new AMap.Size(20, 20),\r\n          }),\r\n          map: this.map,\r\n          position: [this.markData[i].jing, this.markData[i].wei],\r\n          extData: this.markData[i],\r\n        });\r\n\r\n        this.markers.push(marker);\r\n\r\n        AMap.event.addListener(marker, \"click\", () => {\r\n          this.title = \"点位详细信息\";\r\n\r\n          this.content =\r\n            \"点位编号：\" +\r\n            this.markData[i].name +\r\n            \"<br/>\" +\r\n            \"PH：\" +\r\n            this.markData[i].x1 +\r\n            \" \" +\r\n            \"<br/>\" +\r\n            \"铜：\" +\r\n            this.markData[i].x2 +\r\n            \" \" +\r\n            \"铬：\" +\r\n            this.markData[i].x3 +\r\n            \" \" +\r\n            \" <br/>\" +\r\n            \"镍：\" +\r\n            this.markData[i].x4 +\r\n            \" \" +\r\n            \"锌：\" +\r\n            this.markData[i].x5 +\r\n            \" \" +\r\n            \" <br/>\" +\r\n            \"铅：\" +\r\n            this.markData[i].x6 +\r\n            \" \" +\r\n            \"镉：\" +\r\n            this.markData[i].x7 +\r\n            \" \" +\r\n            \"<br/>\" +\r\n            \"砷：\" +\r\n            this.markData[i].x8 +\r\n            \" \" +\r\n            \"汞：\" +\r\n            this.markData[i].x9;\r\n\r\n          this.infoWindow = new AMap.InfoWindow({\r\n            isCustom: true, //使用自定义窗体\r\n            content: this.createInfoWindow(this.title, this.content),\r\n            offset: new AMap.Pixel(16, -45),\r\n          });\r\n          this.infoWindow.open(this.map, marker.getPosition());\r\n        });\r\n      }\r\n\r\n      // this.map.setFitView();\r\n    },\r\n\r\n    //渲染弹框\r\n    createInfoWindow(title, content) {\r\n      var info = document.createElement(\"div\");\r\n      info.className = \"custom-info input-card content-window-card\";\r\n\r\n      //可以通过下面的方式修改自定义窗体的宽高\r\n      //info.style.width = \"400px\";\r\n      // 定义顶部标题\r\n      var top = document.createElement(\"div\");\r\n      var titleD = document.createElement(\"div\");\r\n      var closeX = document.createElement(\"img\");\r\n      top.className = \"info-top\";\r\n      titleD.innerHTML = title;\r\n      closeX.src = \"https://webapi.amap.com/images/close2.gif\";\r\n      closeX.onclick = this.closeInfoWindow;\r\n\r\n      top.appendChild(titleD);\r\n      top.appendChild(closeX);\r\n      info.appendChild(top);\r\n      // 定义中部内容\r\n      var middle = document.createElement(\"div\");\r\n      middle.className = \"info-middle\";\r\n      middle.style.backgroundColor = \"white\";\r\n      middle.innerHTML = content;\r\n      info.appendChild(middle);\r\n\r\n      // 定义底部内容\r\n      var bottom = document.createElement(\"div\");\r\n      bottom.className = \"info-bottom\";\r\n      bottom.style.position = \"relative\";\r\n      bottom.style.top = \"0px\";\r\n      bottom.style.margin = \"0 auto\";\r\n      var sharp = document.createElement(\"img\");\r\n      sharp.src = \"https://webapi.amap.com/images/sharp.png\";\r\n      bottom.appendChild(sharp);\r\n      info.appendChild(bottom);\r\n      return info;\r\n    },\r\n\r\n    // 关闭弹框\r\n    closeInfoWindow() {\r\n      this.map.clearInfoWindow();\r\n      this.searchIndustry = null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.data-manage {\r\n  background: #f7f7f7;\r\n}\r\n\r\n.tablecard {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n}\r\n.tablecard2 {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  height: calc(100vh - 120px);\r\n  background: #fff;\r\n  border: 1px solid #ccc;\r\n}\r\n.box-card {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 322px;\r\n}\r\n\r\n/* 弹框样式 */\r\n.content-window-card {\r\n  position: relative;\r\n  box-shadow: none;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: auto;\r\n  padding: 0;\r\n}\r\n\r\n.content-window-card p {\r\n  height: 2rem;\r\n}\r\n\r\n.custom-info {\r\n  border: solid 1px silver;\r\n  width: 280px;\r\n  background-color: white;\r\n}\r\n\r\ndiv.info-top {\r\n  position: relative;\r\n  background: none repeat scroll 0 0 #f9f9f9;\r\n  border-bottom: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n}\r\n\r\ndiv.info-top div {\r\n  display: inline-block;\r\n  color: #333333;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  line-height: 31px;\r\n  padding: 0 10px;\r\n}\r\n\r\ndiv.info-top img {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  transition-duration: 0.25s;\r\n}\r\n\r\ndiv.info-middle {\r\n  font-size: 12px;\r\n  padding: 10px 6px;\r\n  line-height: 20px;\r\n}\r\ndiv.info-middle img {\r\n  width: 100px;\r\n  height: 70px;\r\n  padding-bottom: 10px;\r\n}\r\n\r\ndiv.info-bottom {\r\n  height: 0px;\r\n  width: 100%;\r\n  clear: both;\r\n  text-align: center;\r\n}\r\n\r\ndiv.info-bottom img {\r\n  position: relative;\r\n  z-index: 104;\r\n}\r\n\r\nspan {\r\n  margin-left: 5px;\r\n  font-size: 11px;\r\n}\r\n\r\n.info-middle img {\r\n  float: left;\r\n  margin-right: 6px;\r\n}\r\n\r\n.xiangxi {\r\n  color: #555;\r\n}\r\n</style>\r\n"]}]}