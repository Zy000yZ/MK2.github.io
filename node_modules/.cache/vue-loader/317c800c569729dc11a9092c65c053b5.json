r />\r\n            <br />\r\n            <el-button type=\"primary\" size=\"small\" @click=\"pgclick\"\r\n              >开始评估</el-button\r\n            >\r\n          </div>\r\n\r\n          <el-table\r\n            border\r\n            stripe\r\n            :data=\"table2Data\"\r\n            style=\"width: 100%\"\r\n            height=\"375\"\r\n          >\r\n            <el-table-column prop=\"id\" label=\"点位编号\"> </el-table-column>\r\n            <el-table-column prop=\"ph\" label=\"PH\"> </el-table-column>\r\n            <el-table-column prop=\"yname\" label=\"元素名\"> </el-table-column>\r\n            <el-table-column prop=\"x\" label=\"元素含量\"> </el-table-column>\r\n          </el-table>\r\n        </div>\r\n        <el-card shadow=\"never\" class=\"box-card\" style=\"width: 120px; z-index: 9999\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>图层切换</span>\r\n          </div>\r\n          <el-radio-group v-model=\"mapType\" @change=\"mapChange\">\r\n            <el-radio label=\"normal\">标准</el-radio><br />\r\n            <el-radio label=\"dark\">幻影黑</el-radio><br />\r\n            <el-radio label=\"light\">月光银</el-radio><br />\r\n            <el-radio label=\"yg\">遥感图</el-radio>\r\n          </el-radio-group>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n\r\n    <el-dialog title=\"数据信息\" :visible.sync=\"dialogTableVisible\" width=\"80%\">\r\n      <el-button type=\"success\" size=\"mini\">新增</el-button>\r\n      <el-table :data=\"tableData\" height=\"400\">\r\n        <!-- <el-table-column\r\n          property=\"dataid\"\r\n          label=\"采样编号\"\r\n          width=\"150\"\r\n        ></el-table-column> -->\r\n        <el-table-column\r\n          property=\"type\"\r\n          label=\"采样类型\"\r\n          width=\"100\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          v-if=\"radio1 === '土壤评估'\"\r\n          property=\"typeonly\"\r\n          label=\"土壤类型\"\r\n          width=\"100\"\r\n        ></