-box\" v-show=\"newCoordinate.length !== 0\">\r\n        <el-card class=\"box-card\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span style=\"font-size: 16px\">垃圾中转清运点数据填写表单</span>\r\n          </div>\r\n          <el-form\r\n            size=\"mini\"\r\n            :model=\"ruleForm\"\r\n            :rules=\"rules\"\r\n            ref=\"ruleForm\"\r\n            label-width=\"150px\"\r\n            class=\"demo-ruleForm\"\r\n          >\r\n            <el-form-item label=\"清运点注册时间\" prop=\"date\">\r\n              <el-input v-model=\"ruleForm.date\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"清运点名称\" prop=\"name\">\r\n              <el-input v-model=\"ruleForm.name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"可堆放垃圾吨数\">\r\n              <el-select\r\n                v-model=\"ruleForm.num\"\r\n                placeholder=\"请选择可堆放垃圾吨数\"\r\n              >\r\n                <el-option label=\"3吨\" value=\"3\"></el-option>\r\n                <el-option label=\"5吨\" value=\"5\"></el-option>\r\n                <el-option label=\"8吨\" value=\"8\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"清运点责任人\" prop=\"admin\">\r\n              <el-input v-model=\"ruleForm.admin\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"责任人联系方式\" prop=\"admintel\">\r\n              <el-input v-model=\"ruleForm.admintel\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"坐标经度\" prop=\"jing\">\r\n              <el-input style=\"width: 200px\" v-model=\"ruleForm.jing\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"坐标纬度\" prop=\"wei\">\r\n              <el-input style=\"width: 200px\" v-model=\"ruleForm.wei\"></el-input>\r\n              <el-button v-if=\"type==='add'\" @click=\"getCoordinateP\" style=\"margin-left: 10px\"\r\n                >重新拾取</el-button\r\n              >\r\n            </el-form-item>\r\n          </el-form>\r\n\r\n          <div class=\"subBtn\">\r\n            <el-button type=\"primary\" @click=\"submitForm('ruleForm')\"\r\n              >立即创建</el-button\r\n            >\r\n            <el-button @click=\"resetForm('ruleForm')\">清空提示</el-button>\r\n            <el-button @click=\"backpage\">返回列表</el-button>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"HeritageAdd-images\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"HeritageAdd\",\r\n\r\n  computed: {\r\n    ...mapState([\"lang\"]),\r\n    ...mapState([\"newCoordinate\", \"instryType\"]),\r\n  },\r\n  async created() {\r\n    const now = new Date();\r\n    const year = now.getFullYear();\r\n    const month = now.getMonth() + 1; // 月份从0开始，需要加1\r\n    const day = now.getDate();\r\n    const hours = now.getHours(); // 获取当前小时，例如：15\r\n    const minutes = now.getMinutes(); // 获取当前分钟，例如：30\r\n    const seconds = now.getSeconds(); // 获取当前秒数，例如：45\r\n\r\n    console.log(year, month, day);\r\n    this.ruleForm.date =\r\n      year +\r\n      \"/\" +\r\n      month +\r\n      \"/\" +\r\n      day +\r\n      \" \" +\r\n      hours +\r\n      \":\" +\r\n      minutes +\r\n      \":\" +\r\n      seconds;\r\n    // 将坐标拾取状态改为add\r\n    console.log(this.newCoordinate);\r\n    this.ruleForm.jing=this.newCoordinate[0]\r\n    this.ruleForm.wei=this.newCoordinate[1]\r\n    this.ruleForm.coon=this.newCoordinate[0]+','+this.newCoordinate[1]\r\n    this.changeCoordinateType(\"add\");\r\n    console.log(this.$route);\r\n    if (this.$route.params.editid !== \"add\") {\r\n      this.type='edit'\r\n      var res = \"\";\r\n        res = await this.$axios.get(\"/LJTMarkDataGet/LJTMarkDataGet\");\r\n          // _id: this.$route.params.editid,\r\n\r\n      console.log(res.data.data);\r\n\r\n      this.ruleForm = res.data.data.find(ele=>ele._id===this.$route.params.editid);\r\n      console.log(\"编辑数据\", this.ruleForm);\r\n      this.getCoordinate([this.ruleForm.jing,this.ruleForm.wei]);\r\n      this.ruleForm.date =\r\n      year +\r\n      \"/\" +\r\n      month +\r\n      \"/\" +\r\n      day +\r\n      \" \" +\r\n      hours +\r\n      \":\" +\r\n      minutes +\r\n      \":\" +\r\n      seconds;\r\n    } \r\n  },\r\n  data() {\r\n    return {\r\n      ruleForm: {\r\n        date: \"\",\r\n        name: \"\",\r\n        num: 3,\r\n        admin: \"\",\r\n        admintel: \"\",\r\n        jing: 0,\r\n        wei: 0,\r\n        coon: \"\",\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: \"请输入清运点名称\", trigger: \"blur\" },\r\n          { min: 3, message: \"长度在不少于3个字符\", trigger: \"blur\" },\r\n        ],\r\n        admin: [\r\n          { required: true, message: \"请输入清运点负责人\", trigger: \"blur\" },\r\n        ],\r\n        admintel: [\r\n          { required: true, message: \"请输入负责人联系方式\", trigger: \"blur\" },\r\n        ],\r\n        date: [\r\n          {\r\n            required: true,\r\n            message: \"请输入日期\",\r\n            trigger: \"change\",\r\n          },\r\n        ],\r\n        jing: [\r\n          {\r\n            required: true,\r\n            message: \"请输入清运点地理坐标\",\r\n            trigger: \"change\",\r\n          },\r\n        ],\r\n        wei: [\r\n          {\r\n            required: true,\r\n            message: \"请输入清运点地理坐标\",\r\n            trigger: \"change\",\r\n          },\r\n        ],\r\n      },\r\n\r\n      type:'add'\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    ...mapMutations([\r\n      \"getCoordinate\",\r\n      \"changeCoordinateType\",\r\n      \"getapplyCoordinate\",\r\n    ]),\r\n\r\n    //跳转页面拾取坐标位置\r\n    getCoordinateP() {\r\n      this.getapplyCoordinate([]);\r\n      this.$router.push(\"/getCoordinateMark\");\r\n    },\r\n\r\n    backpage(){\r\n      this.$router.push(\"/DataMarkList\");\r\n    },\r\n\r\n    submitForm(formName) {\r\n      console.log(formName);\r\n      console.log(this.ruleForm);\r\n      this.$refs[formName].validate(async (valid) => {\r\n        if (valid) {\r\n          // 格式化数据\r\n          this.ruleForm.coon = this.ruleForm.jing + `,` + this.ruleForm.wei;\r\n          // 格式已经处理完毕，下面发送请求\r\n          console.log(this.ruleForm);\r\n          // 发送请求;\r\n          if (this.$route.params.editid !== \"add\") {\r\n            var res = await this.$axios.post(\r\n              \"/LJTMarkDataEdit/LJTMarkDataEdit\",\r\n              this.ruleForm\r\n            );\r\n            console.log(\"res====\", res);\r\n            if (res.status === 200) {\r\n              this.$message({\r\n                message: \"编辑成功\",\r\n                type: \"success\",\r\n              });\r\n              this.getCoordinate([]); //将拾取到的坐标清空\r\n              this.$router.push(\"/DataMarkList\");\r\n            }\r\n          } else {\r\n            console.log(\"ruleForm\", this.ruleForm);\r\n            var res2 = await this.$axios.post(\r\n              \"/LJTMarkDataAdd/LJTMarkDataAdd\",\r\n              this.ruleForm\r\n            );\r\n            if (res2.status === 200) {\r\n              this.$notify({\r\n                title: \"垃圾中转清运点信息添加成功\",\r\n              });\r\n              this.getCoordinate([]);\r\n              this.$router.push(\"/DataMarkList\");\r\n            }\r\n          }\r\n        } else {\r\n          this.$message.error(\"请检查完善相关信息\");\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.HeritageAdd-bottom {\r\n  background-color: white;\r\n  margin: 20px;\r\n}\r\n</style>\r\n"]}]}