{"remainingRequest":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\src\\views\\LanPointMap.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\src\\views\\LanPointMap.vue","mtime":1728893473921},{"path":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Code Files\\VueFile\\长期开发工作\\光缆资源平面展示系统（临时）\\webgis-gx-cp1.0\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["LanPointMap.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAydA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"LanPointMap.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"heritage-industry\">\r\n    <!-- 地图容器 -->\r\n    <div class=\"map-buttons\">\r\n      <!-- 区域搜索start -->\r\n      <!-- <el-select\r\n        size=\"mini\"\r\n        @change=\"changeDistrict\"\r\n        v-model=\"currentQu.qu\"\r\n        placeholder=\"请选择\"\r\n        style=\"width: 150px; margin-right: 15px\"\r\n      >\r\n        <el-option\r\n          v-for=\"item in cityCode\"\r\n          :key=\"item.qu\"\r\n          :label=\"item.qu\"\r\n          :value=\"item.qu\"\r\n        >\r\n        </el-option>\r\n      </el-select> -->\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addRoadLayer\"\r\n        v-show=\"!roadNetwork\"\r\n        class=\"map-btn map-element\"\r\n        style=\"margin-left: 15px\"\r\n        >{{ lang === \"zh_cn\" ? \"打开路网\" : \"Open road\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"removeRoadLayer\"\r\n        v-show=\"roadNetwork\"\r\n        class=\"map-btn map-element\"\r\n        style=\"margin-left: 15px\"\r\n        >{{ lang === \"zh_cn\" ? \"关闭路网\" : \"Close road\" }}</el-button\r\n      >\r\n\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"addSatellite\"\r\n        v-show=\"isOrdMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"卫星地图\" : \"Satellite map\" }}</el-button\r\n      >\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"removeSatellite\"\r\n        v-show=\"!isOrdMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"普通地图\" : \"General map\" }}</el-button\r\n      >\r\n\r\n      <el-button\r\n        type=\"text\"\r\n        size=\"mini\"\r\n        @click=\"resetMap\"\r\n        class=\"map-btn map-element\"\r\n        >{{ lang === \"zh_cn\" ? \"刷新\" : \"Reset\" }}</el-button\r\n      >\r\n    </div>\r\n    <div id=\"container\" class=\"container\">\r\n      <div id=\"panel\"></div>\r\n      <div class=\"egColor\">\r\n        <p>图例</p>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/电杆0818.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>电杆</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/人井0812.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>人井</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/挂墙点0819.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>挂墙点</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/光缆引上点.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>光缆引上点</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/资源点.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>光交箱</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/资源点.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>机房</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <img\r\n            style=\"width: 38px; height: 38px\"\r\n            src=\"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/资源点.png\"\r\n            alt=\"\"\r\n          />\r\n          <span>资源点</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"egBox\">\r\n        <p>缆线信息面板</p>\r\n        <div class=\"egColor-che\">\r\n          <span>缆线名称： {{ egBoxData.markName }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>缆线长度（千米）： {{ egBoxData.linelength }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>缆线纤芯： {{ egBoxData.method }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>缆线属性： {{ egBoxData.property }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>资源属性： {{ egBoxData.project }}</span>\r\n        </div>\r\n\r\n        <div class=\"egColor-che\">\r\n          <span>所属区域： {{ egBoxData.districtArr[0] }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>产权单位： {{ egBoxData.company }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>负责人： {{ egBoxData.admin }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>通过点位数： {{ egBoxData.pointArr.length }}</span>\r\n        </div>\r\n        <div class=\"egColor-che\">\r\n          <span>数据更新时间： {{ egBoxData.editData }}</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nexport default {\r\n  name: \"HeritageIndustry\",\r\n\r\n  data() {\r\n    return {\r\n      map: \"\",\r\n      district: null,\r\n      polygons: [],\r\n      zoom: 12,\r\n      polygon: [], //点位\r\n\r\n      // 弹框内容\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n\r\n      dataList: null, // 数据\r\n      mainData: null,\r\n      lineData: [],\r\n      mainLineData: [],\r\n\r\n      isOrdMap: true, //判断是卫星图还是普通图，普通图为true\r\n      satellite: null, //用来存储卫星地图影像\r\n      roadNetwork: false, //是否具有路网，默认没有\r\n      roadNetLayer: null, //用来存储路网\r\n\r\n      searchIndustry: \"\",\r\n      markers: [],\r\n      liners: [],\r\n\r\n      // 缆线信息面板初始化信息\r\n      egBoxData: {\r\n        markName: \"暂无数据\",\r\n        linelength: 0,\r\n        address: \"暂无数据\",\r\n        method: \"暂无数据\",\r\n        project: \"暂无数据\",\r\n        property: \"暂无数据\",\r\n        districtArr: [\"暂无数据\"],\r\n        state: \"暂无数据\",\r\n        start: \"暂无数据\",\r\n        editData: \"暂无数据\",\r\n        company: \"暂无数据\",\r\n        admin: \"暂无数据\",\r\n        addressObject: {\r\n          adcode: \"310101\",\r\n          district: \"黄浦区\",\r\n          province: \"上海市\",\r\n          street: \"南京路步行街\",\r\n          streetNumber: \"539号\",\r\n          township: \"外滩街道\",\r\n          towncode: \"310101013000\",\r\n        },\r\n        pointArr: [],\r\n      },\r\n\r\n      typeSelect: \"所有停车场\",\r\n\r\n      options: [\r\n        {\r\n          name: \"所有停车场\",\r\n        },\r\n      ],\r\n\r\n      currentQu: {\r\n        qu: \"黄浦区\",\r\n        township: \"全部街道\",\r\n      },\r\n      districtList: [],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapState([\"lang\", \"nowAddress\", \"districtObj\", \"cityCode\"]),\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap(); //初始化地图\r\n    this.mouseTool = new AMap.MouseTool(this.map);\r\n    this.getQuLintData();\r\n    this.getQuPointData();\r\n  },\r\n\r\n  methods: {\r\n    initMap() {\r\n      this.map = new AMap.Map(\"container\", {\r\n        resizeEnable: true,\r\n        zoom: 14, //级别\r\n        zooms: [3, 30],\r\n        center: [121.47, 31.23], //中心点坐标\r\n        viewMode: \"3D\", //使用3D视图\r\n        lang: this.lang,\r\n        mapStyle: \"amap://styles/macaron\",\r\n        infoWindow: null,\r\n      });\r\n    },\r\n\r\n    // 获取点数据\r\n    async getQuPointData() {\r\n      var qucode = this.cityCode.find((ele) => ele.qu === this.currentQu.qu);\r\n      var res = null;\r\n      var str = \"/LanPoint\" + '01' + \"Get\";\r\n      res = await this.$axios.get(str + str);\r\n      if (res.status === 200) {\r\n        console.log(\"success\", this.currentQu.qu, res);\r\n        this.dataList = res.data.data;\r\n        this.restaurant = res.data.data;\r\n        this.addMarker();\r\n      }\r\n    },\r\n\r\n    // 获取线数据\r\n    async getQuLintData() {\r\n      var qucode = this.cityCode.find((ele) => ele.qu === this.currentQu.qu);\r\n      var res = null;\r\n      var str = \"/LanLine\" + qucode.code + \"Get\";\r\n      res = await this.$axios.get(str + str);\r\n      if (res.status === 200) {\r\n        console.log(\"success\", this.currentQu.qu, res);\r\n        this.lineData = res.data.data;\r\n        this.mainLineData = res.data.data;\r\n      }\r\n    },\r\n    changeDistrict() {\r\n      this.getQuPointData();\r\n    },\r\n\r\n    addLine() {\r\n      if(this.liners!==0)this.map.remove(this.liners);\r\n      this.liners = [];\r\n      for (let i = 0; i < this.lineData.length; i++) {\r\n        var iscolor = \"#99672F\";\r\n        if (this.lineData[i].property === \"飞线\") iscolor = \"#6B6B64\";\r\n        if (this.lineData[i].property === \"引上线\") iscolor = \"#555\";\r\n\r\n        let path = this.lineData[i].pointArr.map((ele) => ele.coon);\r\n\r\n        let liner = new AMap.Polyline({\r\n          path: path,\r\n          isOutline: false,\r\n          outlineColor: \"#ffeeff\",\r\n          borderWeight: 2,\r\n          strokeColor: iscolor,\r\n          strokeOpacity: 1,\r\n          strokeWeight: 2,\r\n          // 折线样式还支持 'dashed'\r\n          strokeStyle: \"solid\",\r\n          // strokeStyle是dashed时有效\r\n          strokeDasharray: [10, 5],\r\n          // lineJoin: \"round\",\r\n          // lineCap: \"round\",\r\n          zIndex: 50,\r\n        });\r\n        AMap.event.addListener(liner, \"click\", () => {\r\n          this.egBoxData = this.lineData[i];\r\n          this.$message({\r\n            message: \"缆线信息面板内容已更新\",\r\n            type: \"success\",\r\n          });\r\n        });\r\n        liner.setMap(this.map);\r\n        this.liners.push(liner);\r\n      }\r\n    },\r\n\r\n    // 添加点集\r\n    addMarker() {\r\n      this.map.clearMap();\r\n      this.markers = [];\r\n      for (let i = 0; i < this.dataList.length; i++) {\r\n        // 注意用 let\r\n        var url =\r\n          \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/资源点.png\";\r\n        if (this.dataList[i].property === \"人井\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/人井0812.png\";\r\n        }\r\n        if (this.dataList[i].property === \"电杆\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/电杆0818.png\";\r\n        }\r\n        if (this.dataList[i].property === \"挂墙点\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/挂墙点0819.png\";\r\n        }\r\n        if (this.dataList[i].property === \"光缆引上点\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/光缆引上点.png\";\r\n        }\r\n\r\n        let marker = new AMap.Marker({\r\n          icon: new AMap.Icon({\r\n            image: url,\r\n            size: new AMap.Size(30, 30),\r\n            imageSize: new AMap.Size(30, 30),\r\n          }),\r\n          map: this.map,\r\n          position: [this.dataList[i].jing, this.dataList[i].wei],\r\n          extData: this.dataList[i],\r\n        });\r\n        this.markers.push(marker);\r\n        AMap.event.addListener(marker, \"click\", () => {\r\n          // console.log('图标被点击了')\r\n          this.title = this.dataList[i].markName;\r\n          (this.content = [\r\n            \"安装地址：\" + this.dataList[i].address,\r\n            \"更新时间：\" + this.dataList[i].editData,\r\n            \"产权单位：\" + this.dataList[i].company,\r\n            \"维护单位：\" + this.dataList[i].method,\r\n            \"负责人：\" + this.dataList[i].admin,\r\n          ]),\r\n            (this.infoWindow = new AMap.InfoWindow({\r\n              isCustom: true, //使用自定义窗体\r\n              content: this.createInfoWindow(\r\n                this.title,\r\n                this.content.join(\"<br/>\")\r\n              ),\r\n              offset: new AMap.Pixel(16, -45),\r\n            }));\r\n          this.infoWindow.open(this.map, marker.getPosition());\r\n        });\r\n      }\r\n      this.map.setFitView();\r\n      this.addLine();\r\n    },\r\n\r\n    //渲染弹框\r\n    createInfoWindow(title, content) {\r\n      var info = document.createElement(\"div\");\r\n      info.className = \"custom-info input-card content-window-card\";\r\n      //可以通过下面的方式修改自定义窗体的宽高\r\n      //info.style.width = \"400px\";\r\n      // 定义顶部标题\r\n      var top = document.createElement(\"div\");\r\n      var titleD = document.createElement(\"div\");\r\n      var closeX = document.createElement(\"img\");\r\n      top.className = \"info-top\";\r\n      titleD.innerHTML = title;\r\n      closeX.src = \"https://webapi.amap.com/images/close2.gif\";\r\n      closeX.onclick = this.closeInfoWindow;\r\n\r\n      top.appendChild(titleD);\r\n      top.appendChild(closeX);\r\n      info.appendChild(top);\r\n\r\n      // 定义中部内容\r\n      var middle = document.createElement(\"div\");\r\n      middle.className = \"info-middle\";\r\n      middle.style.backgroundColor = \"white\";\r\n      middle.innerHTML = content;\r\n      info.appendChild(middle);\r\n\r\n      // 定义底部内容\r\n      var bottom = document.createElement(\"div\");\r\n      bottom.className = \"info-bottom\";\r\n      bottom.style.position = \"relative\";\r\n      bottom.style.top = \"0px\";\r\n      bottom.style.margin = \"0 auto\";\r\n      var sharp = document.createElement(\"img\");\r\n      sharp.src = \"https://webapi.amap.com/images/sharp.png\";\r\n      bottom.appendChild(sharp);\r\n      info.appendChild(bottom);\r\n      return info;\r\n    },\r\n    // 关闭弹框\r\n    closeInfoWindow() {\r\n      this.map.clearInfoWindow();\r\n      this.searchIndustry = null;\r\n    },\r\n    // 打开卫星地图\r\n    addSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.satellite = new AMap.TileLayer.Satellite();\r\n      this.map.add(this.satellite);\r\n    },\r\n\r\n    // 关闭卫星地图\r\n    removeSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.map.remove(this.satellite);\r\n    },\r\n\r\n    // 打开路网\r\n    addRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.roadNetLayer = new AMap.TileLayer.RoadNet();\r\n      this.map.add(this.roadNetLayer);\r\n    },\r\n\r\n    // 关闭路网\r\n    removeRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.map.remove(this.roadNetLayer);\r\n    },\r\n\r\n    // 复位事件\r\n    resetMap() {\r\n      location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.egBox {\r\n  background-color: white;\r\n  position: fixed;\r\n  top: 155px;\r\n  left: 225px;\r\n  z-index: 99;\r\n\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n  padding: 15px;\r\n}\r\n.egColor {\r\n  /* width: 300px;\r\n  height: 400px; */\r\n  background-color: white;\r\n  position: fixed;\r\n  top: 155px;\r\n  right: 30px;\r\n  z-index: 99;\r\n\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n  padding: 15px;\r\n}\r\n\r\n.egColor .egColor-che {\r\n  display: flex;\r\n  margin-top: 10px;\r\n  align-items: center;\r\n}\r\n\r\n.heritage-industry #container {\r\n  border-bottom: 1px solid #f9d5a7;\r\n}\r\n/* 弹框样式 */\r\n.content-window-card {\r\n  position: relative;\r\n  box-shadow: none;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: auto;\r\n  padding: 0;\r\n}\r\n\r\n.content-window-card p {\r\n  height: 2rem;\r\n}\r\n\r\n.custom-info {\r\n  border: solid 1px silver;\r\n  width: 280px;\r\n  background-color: white;\r\n}\r\n\r\ndiv.info-top {\r\n  position: relative;\r\n  background: none repeat scroll 0 0 #f9f9f9;\r\n  border-bottom: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n}\r\n\r\ndiv.info-top div {\r\n  display: inline-block;\r\n  color: #333333;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  line-height: 31px;\r\n  padding: 0 10px;\r\n}\r\n\r\ndiv.info-top img {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  transition-duration: 0.25s;\r\n}\r\n\r\ndiv.info-middle {\r\n  font-size: 12px;\r\n  padding: 10px 6px;\r\n  line-height: 20px;\r\n}\r\ndiv.info-middle img {\r\n  width: 100px;\r\n  height: 70px;\r\n  padding-bottom: 10px;\r\n}\r\n\r\ndiv.info-bottom {\r\n  height: 0px;\r\n  width: 100%;\r\n  clear: both;\r\n  text-align: center;\r\n}\r\n\r\ndiv.info-bottom img {\r\n  position: relative;\r\n  z-index: 104;\r\n}\r\n\r\nspan {\r\n  margin-left: 5px;\r\n  font-size: 11px;\r\n}\r\n\r\n.info-middle img {\r\n  float: left;\r\n  margin-right: 6px;\r\n}\r\n\r\n.xiangxi {\r\n  color: #555;\r\n}\r\n.amap-icon {\r\n  /* margin-bottom: -5px; */\r\n  margin-top: 15px;\r\n}\r\n/* 地图元件样式 */\r\n\r\n.heritage-industry .map-buttons {\r\n  padding: 15px;\r\n  z-index: 999;\r\n  position: re