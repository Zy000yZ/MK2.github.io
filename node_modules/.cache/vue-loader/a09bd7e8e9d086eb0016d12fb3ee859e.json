class=\"data-manage-buttons\"\r\n        style=\"\r\n          background: #fff;\r\n          border: 1px solid #ccc;\r\n          height: calc(100vh - 150px);\r\n        \"\r\n      ></div>\r\n\r\n      <div style=\"position: fixed; top: 76px; right: 15px\">\r\n        <div class=\"tablecard2\" style=\"width: 320px; z-index: 999\">\r\n          <div style=\"padding: 10px; box-sizing: border-box\">\r\n            <el-upload\r\n              :on-change=\"changeFile\"\r\n              class=\"upload-demo\"\r\n              drag\r\n              show-file-list\r\n              action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n              multiple\r\n            >\r\n              <i class=\"el-icon-upload\"></i>\r\n              <div class=\"el-upload__text\">\r\n                将文件拖到此处，或<em>点击上传</em>\r\n              </div>\r\n              <div class=\"el-upload__tip\" slot=\"tip\">\r\n                上传遥感文件进行提取分析\r\n              </div>\r\n            </el-upload>\r\n            <br />\r\n            <el-tag type=\"warning\" v-if=\"isFile\"\r\n              >浏览器存储已上传遥感文件</el-tag\r\n            >\r\n            <el-tag type=\"danger\" v-else>文件未上传</el-tag>\r\n          </div>\r\n\r\n          <div style=\"padding: 10px\">\r\n            <el-button type=\"primary\" @click=\"drawPoly\" v-if=\"isFile\"\r\n              >提取茶园数据</el-button\r\n            >\r\n            <br />\r\n            <br />\r\n            <el-button type=\"primary\" @click=\"fuwei\">复位地图</el-button>\r\n            <el-button type=\"primary\" @click=\"drawBounds\"\r\n              >展示行政区边界</el-button\r\n            >\r\n            <br />\r\n            <br />\r\n            <el-button type=\"primary\" @click=\"drawpngmap\"\r\n              >叠加高分辨率影像</el-button\r\n            >\r\n          </div>\r\n        </div>\r\n        <el-card\r\n          shadow=\"never\"\r\n          class=\"box-card\"\r\n          style=\"width: 120px; z-index: 9999\"\r\n        >\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>图层切换</span>\r\n          </div>\r\n          <el-radio-group v-model=\"mapType\" @change=\"mapChange\">\r\n            <el-radio label=\"normal\">标准</el-radio><br />\r\n            <el-radio label=\"dark\">幻影黑</el-radio><br />\r\n            <el-radio label=\"light\">月光银</el-radio><br />\r\n            <el-radio label=\"yg\">遥感图</el-radio>\r\n          </el-radio-group>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport Imgqu15 from \"../assets/Cache_41b4f3512ccf68f1..png\";\r\n\r\nimport { mapState } from \"vuex\";\r\nexport default {\r\n  computed: {\r\n    ...mapState([\"markData2\"]),\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      markData: [],\r\n      dialogTableVisible: false,\r\n\r\n      mapType: \"yg\",\r\n      map: \"\",\r\n      marker: null, //点位\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n      markers: [],\r\n      satellite: null,\r\n\r\n      isPG: false,\r\n      scelOption: {\r\n        scl1: \"杭州市\",\r\n        scl2: \"原始影像\",\r\n      },\r\n\r\n      dataList: [],\r\n      polygons: [],\r\n      pathArr: null,\r\n      isFile: false,\r\n      isPush: false,\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.changePngMap();\r\n    this.mapChange();\r\n  },\r\n\r\n  created() {\r\n    if (sessionStorage.getItem(\"isfiled\") === \"文件已上传\") {\r\n      this.isFile = true;\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    changeFile() {\r\n      console.log(\"文件上传\");\r\n      this.isFile = true;\r\n      sessionStorage.setItem(\"isfiled\", \"文件已上传\");\r\n    },\r\n\r\n    // 裁剪边界\r\n    drawBounds() {\r\n      new AMap.DistrictSearch({\r\n        // level:'district',\r\n        extensions: \"all\",\r\n        subdistrict: 1,\r\n      }).search(\"341003\", (status, result) => {\r\n        console.log(status, result);\r\n        // 外多边形坐标数组和内多边形坐标数组\r\n        var outer = [\r\n          new AMap.LngLat(-360, 90, true),\r\n          new AMap.LngLat(-360, -90, true),\r\n          new AMap.LngLat(360, -90, true),\r\n          new AMap.LngLat(360, 90, true),\r\n        ];\r\n        var holes = result.districtList[0].boundaries;\r\n\r\n        var pathArray = [outer];\r\n        pathArray.push.apply(pathArray, holes);\r\n        var polygon = new AMap.Polygon({\r\n          pathL: pathArray,\r\n          strokeColor: \"#00eeff\",\r\n          strokeWeight: 1,\r\n          fillColor: \"#71B3ff\",\r\n          fillOpacity: 0.5,\r\n        });\r\n        polygon.setPath(pathArray);\r\n        this.map.add(polygon);\r\n      });\r\n    },\r\n\r\n    drawPoly() {\r\n      this.isPush = true;\r\n      setTimeout(() => {\r\n        this.isPush = false;\r\n        this.addPol();\r\n      }, 5000);\r\n    },\r\n\r\n    fuwei() {\r\n      this.map.setZoomAndCenter(12, [118.271291, 30.16449]);\r\n    },\r\n\r\n    mapChange() {\r\n      if (this.mapType === \"yg\") {\r\n        this.satellite = new AMap.TileLayer.Satellite();\r\n        this.map.add(this.satellite);\r\n      } else {\r\n        if (this.satellite) {\r\n          this.map.remove(this.satellite);\r\n        }\r\n        var styleName = \"amap://styles/\" + this.mapType;\r\n        this.map.setMapStyle(styleName);\r\n      }\r\n    },\r\n\r\n    changePngMap() {\r\n      this.map = new AMap.Map(\"container0307\", {\r\n        resizeEnable: true,\r\n        zoom: 12, //级别\r\n        zooms: [8, 30],\r\n        center: [118.271291, 30.16449], //中心点坐标\r\n        viewMode: \"2D\", //使用3D视图\r\n        mapStyle: \"amap://styles/normal\",\r\n        infoWindow: null,\r\n      });\r\n      this.mapType = \"yg\";\r\n    },\r\n\r\n    drawpngmap() {\r\n      var imageLayer = new AMap.ImageLayer({\r\n        url: Imgqu15,\r\n        bounds: new AMap.Bounds([118.15, 30.064], [118.366, 30.3]),\r\n        // bounds: new AMap.Bounds([118.15, 30.06667], [118.36667, 30.3]),\r\n      });\r\n\r\n      this.map = new AMap.Map(\"container0307\", {\r\n        resizeEnable: true,\r\n        zoom: 12, //级别\r\n        zooms: [8, 30],\r\n        center: [118.271291, 30.16449], //中心点坐标\r\n        viewMode: \"2D\", //使用3D视图\r\n        mapStyle: \"amap://styles/normal\",\r\n        infoWindow: null,\r\n        layers: [new AMap.TileLayer(), imageLayer],\r\n      });\r\n      this.mapType = \"yg\";\r\n\r\n      this.mapChange();\r\n    },\r\n\r\n    async addPol() {\r\n      var res = await this.$axios.get(\"/ChaYuanDataGetData/ChaYuanDataGetData\");\r\n      this.dataList = res.data.data;\r\n\r\n      for (let i = 0; i < this.dataList.length; i++) {\r\n        var path = this.dataList[i].path.map((ele) => [ele.lng, ele.lat]);\r\n\r\n        var colorType = {\r\n          strokeColor: \"#BF360C\",\r\n          strokeWeight: 6,\r\n          strokeOpacity: 0.2,\r\n          fillOpacity: 0.4,\r\n          fillColor: \"#BF360C\",\r\n        };\r\n\r\n        let polygon = new AMap.Polygon({\r\n          path: path,\r\n          ...colorType,\r\n        });\r\n        this.polygons.push(polygon);\r\n      }\r\n\r\n      this.map.add(this.polygons);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.el-upload-dragger {\r\n  width: 300px;\r\n}\r\n\r\n.data-manage {\r\n  background: #f7f7f7;\r\n}\r\n\r\n.tablecard2 {\r\n  height: calc(100vh - 151px);\r\n  background: #fff;\r\n  border: 1px solid #ccc;\r\n  z-index: 999;\r\n}\r\n.box-card {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 322px;\r\n  border-radius: 0;\r\n}\r\n.box-card2 {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0px;\r\n  border-radius: 0;\r\n  height: 140px;\r\n}\r\n</style>\r\n"]}]}