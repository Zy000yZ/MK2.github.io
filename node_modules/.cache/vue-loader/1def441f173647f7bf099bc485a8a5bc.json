r\n      // 表单信息\r\n      dialogFormVisible: false,\r\n      form: {\r\n        name: \"\",\r\n        path: [],\r\n        mianji: 0,\r\n        adddate: \"\",\r\n        parknum: 1,\r\n        parkmoney: 1,\r\n        parkadmin: \"\",\r\n        parktel: \"\",\r\n        pl:[],\r\n      },\r\n      formLabelWidth: \"120px\",\r\n      nowUser: \"暂未获取用户数据\",\r\n    };\r\n  },\r\n\r\n  created () {\r\n    this.nowUser = sessionStorage.getItem(\"username\");\r\n    console.log(this.nowUser);\r\n  },\r\n\r\n  computed: {\r\n    ...mapState([\"lang\", \"nowAddress\"]),\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap(); //初始化地图\r\n    this.mouseTool = new AMap.MouseTool(this.map);\r\n    this.addPol();\r\n  },\r\n\r\n  methods: {\r\n    searchTypeClick() {\r\n      this.map.clearMap();\r\n      this.polygons = [];\r\n\r\n      var newArray = [];\r\n      if (this.typeSelect === \"所有停车场\") {\r\n        newArray = this.dataList;\r\n      } else {\r\n        newArray = this.dataList.filter((ele) => ele.name === this.typeSelect);\r\n        this.form = newArray[0];\r\n        console.log(\"this.form\", this.form);\r\n      }\r\n\r\n      for (let i = 0; i < newArray.length; i++) {\r\n        var path = newArray[i].path.map((ele) => [ele.R, ele.Q]);\r\n\r\n        var colorType = {\r\n          fillColor: \"#1976D2\", // 多边形填充颜色\r\n          borderWeight: 2, // 线条宽度，默认为 1\r\n          strokeColor: \"#0D47A1\", // 线条颜色林地，绿地，湿地\r\n          strokeWeight: 6,\r\n          strokeOpacity: 0.2,\r\n          fillOpacity: 0.8,\r\n          strokeStyle: \"solid\",\r\n        };\r\n\r\n        let polygon = new AMap.Polygon({\r\n          path: path,\r\n          ...colorType,\r\n        });\r\n\r\n        polygon.on(\"click\", () => {\r\n          console.log(\"tets--\", newArray[i]);\r\n          this.egBoxData = {\r\n            name: newArray[i].name,\r\n            mianji: Math.floor(newArray[i].mianji / 10),\r\n            path: newArray[i].path.length,\r\n            adddate: newArray[i].adddate,\r\n            parkadmin: newArray[i].parkadmin,\r\n            parktel: newArray[i].parktel,\r\n            parkmoney: newArray[i].parkmoney,\r\n            parknum: newArray[i].parknum,\r\n          };\r\n        });\r\n        this.polygons.push(polygon);\r\n      }\r\n\r\n      this.map.add(this.polygons);\r\n      this.map.setFitView();\r\n    },\r\n\r\n    initMap() {\r\n      //初始化地图\r\n      this.map = new AMap.Map(\"container\", {\r\n        resizeEnable: true,\r\n        zoom: 14, //级别\r\n        zooms: [11, 30],\r\n        center: [118.18, 39.63], //中心点坐标\r\n        viewMode: \"3D\", //使用3D视图\r\n        lang: this.lang,\r\n        mapStyle: \"amap://styles/fresh\",\r\n        infoWindow: null,\r\n      });\r\n    },\r\n\r\n    editPark() {\r\n      if (!this.typeSelect) {\r\n        this.$message.error(\"请先选择要维护的停车场\");\r\n      } else {\r\n        this.searchTypeClick();\r\n        this.dialogFormVisible = true;\r\n      }\r\n    },\r\n\r\n    async editParkPost() {\r\n      console.log(\"post\", this.form);\r\n      var res = null;\r\n      res = await this.$axios.post(\r\n        \"/TSParkDataEditData/TSParkDataEditData\",\r\n        this.form\r\n      );\r\n      if (res.status === 200) {\r\n        this.$message({\r\n          message: \"停车场数据维护成功\",\r\n          type: \"success\",\r\n        });\r\n        this.dialogFormVisible=false\r\n      }\r\n    },\r\n\r\n    async addPol() {\r\n      var res = await this.$axios.get(\"/TSParkDataGet/TSParkDataGet\");\r\n\r\n      if(this.nowUser==='admin'){\r\n        this.dataList = res.data.data;\r\n      }else{\r\n        this.dataList=res.data.data.filter (ele=>{\r\n        console.log(res,this.nowUser)\r\n          return ele.parkadmin===this.nowUser\r\n        })\r\n      }\r\n\r\n      var arr = this.dataList.map((ele) => {\r\n        return {\r\n          name: ele.name,\r\n        };\r\n      });\r\n      this.restaurant = this.dataList;\r\n      this.options = [...this.options, ...arr];\r\n\r\n    },\r\n    // 打开卫星地图\r\n    addSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.satellite = new AMap.TileLayer.Satellite();\r\n      this.map.add(this.satellite);\r\n    },\r\n\r\n    // 关闭卫星地图\r\n    removeSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.map.remove(this.satellite);\r\n    },\r\n\r\n    // 打开路网\r\n    addRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.roadNetLayer = new AMap.TileLayer.RoadNet();\r\n      this.map.add(this.roadNetLayer);\r\n    },\r\n\r\n    // 关闭路网\r\n    removeRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.map.remove(this.roadNetLayer);\r\n    },\r\n\r\n    // 复位事件\r\n    resetMap() {\r\n      location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.egBox {\r\n  background-color: white;\r\n  position: fixed;\r\n  top: 155px;\r\n  left: 225px;\r\n  z-index: 99;\r\n\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n  padding: 15px;\r\n}\r\n.egColor {\r\n  /* width: 300px;\r\n  height: 400px; */\r\n  background-color: white;\r\n  position: fixed;\r\n  top: 155px;\r\n  right: 30px;\r\n  z-index: 99;\r\n\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n  padding: 15px;\r\n}\r\n\r\n.egColor .egColor-che {\r\n  display: flex;\r\n  margin-top: 10px;\r\n}\r\n.egColor .egColor-che div {\r\n  width: 45px;\r\n  height: 20px;\r\n  margin-right: 10px;\r\n}\r\n\r\n.heritage-industry #container {\r\n  border-bottom: 1px solid #f9d5a7;\r\n}\r\n/* 弹框样式 */\r\n.content-window-card {\r\n  position: relative;\r\n  box-shadow: none;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: auto;\r\n  padding: 0;\r\n}\r\n\r\n.content-window-card p {\r\n  height: 2rem;\r\n}\r\n\r\n.custom-info {\r\n  border: solid 1px silver;\r\n  width: 280px;\r\n  background-color: white;\r\n}\r\n\r\ndiv.info-top {\r\n  position: relative;\r\n  background: none repeat scroll 0 0 #f9f9f9;\r\n  border-bottom: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n}\r\n\r\ndiv.info-top div {\r\n  display: inline-block;\r\n  color: #333333;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  line-height: 31px;\r\n  padding: 0 10px;\r\n}\r\n\r\ndiv.info-top img {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  transition-duration: 0.25s;\r\n}\r\n\r\ndiv.info-middle {\r\n  font-size: 12px;\r\n  padding: 10px 6px;\r\n  line-height: 20px;\r\n}\r\ndiv.info-middle img {\r\n  width: 100px;\r\n  height: 70px;\r\n  padding-bottom: 10px;\r\n}\r\n\r\ndiv.info-bottom {\r\n  height: 0px;\r\n  width: 100%;\r\n  clear: both;\r\n  text-align: center;\r\n}\r\n\r\ndiv.info-bottom img {\r\n  position: relative;\r\n  z-index: 104;\r\n}\r\n\r\nspan {\r\n  margin-left: 5px;\r\n  font-size: 11px;\r\n}\r\n\r\n.info-middle img {\r\n  float: left;\r\n  margin-right: 6px;\r\n}\r\n\r\n.xiangxi {\r\n  color: #555;\r\n}\r\n\r\n/* 地图元件样式 */\r\n\r\n.heritage-industry .map-buttons {\r\n  padding: 15px;\r\n  z-index: 999;\r\n  position: relative;\r\n  /* position: absolute;\r\n  top: 0;\r\n  right: 0; */\r\n}\r\n\r\n.heritage-industry .geometrySearch {\r\n  position: absolute;\r\n  top: 60px;\r\n  right: 0;\r\n  z-index: 999;\r\n  background-color: white;\r\n  padding: 15px;\r\n  border-radius: 5px;\r\n  margin-right: 15px;\r\n  border: 1px solid #dcdfe6;\r\n}\r\n\r\n.heritage-industry .geometrySearch .geometryType {\r\n  display: block;\r\n  padding-bottom: 15px;\r\n  color: #606266;\r\n}\r\n.heritage-industry .geometrySearch .geometryBtns {\r\n  padding-top: 15px;\r\n}\r\n\r\n.heritage-industry .map-buttons .closegeometryBox {\r\n  margin-left: 0;\r\n}\r\n\r\n.heritage-industry .geometrySearch .searchRes {\r\n  padding-top: 15px;\r\n}\r\n\r\n.heritage-industry .geometrySearch .searchRes .el-form-item__label {\r\n  font-size: 12px;\r\n  color: #111;\r\n}\r\n.heritage-industry .geometrySearch .searchRes .el-form-item {\r\n  margin: 0;\r\n  padding-left: 10px;\r\n}\r\n.heritage-industry .map-buttons .el-input__inner {\r\n  border-top-right-radius: 0px;\r\n  border-bottom-right-radius: 0px;\r\n  border-right: none;\r\n}\r\n.heritage-industry .map-buttons .selectBtn {\r\n  border-top-left-radius: 0px;\r\n  border-bottom-left-radius: 0px;\r\n\r\n  margin-top: -5px !important;\r\n  position: absolute;\r\n  top: 22px;\r\n}\r\n</style>\r\n"]}]}