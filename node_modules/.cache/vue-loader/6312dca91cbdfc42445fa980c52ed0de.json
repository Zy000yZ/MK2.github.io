\n        ><i style=\"padding-right: 15px\" class=\"el-icon-s-home\"></i>\r\n      </div>\r\n      <div>\r\n        <el-radio-group v-model=\"radio1\" size=\"small\">\r\n          <el-radio-button label=\"土壤评估\"></el-radio-button>\r\n          <el-radio-button label=\"农作物协同评估\"></el-radio-button>\r\n        </el-radio-group>\r\n      </div>\r\n    </div> -->\r\n    <div class=\"data-manage-bottom padding15\">\r\n      <div\r\n        id=\"container0307\"\r\n        class=\"data-manage-buttons\"\r\n        style=\"\r\n          background: #fff;\r\n          border: 1px solid #ccc;\r\n          height: calc(100vh - 150px);\r\n        \"\r\n      ></div>\r\n\r\n      <div style=\"position: fixed; top: 76px; right: 15px\">\r\n        <div class=\"tablecard2\" style=\"width: 320px; z-index: 999\">\r\n          <div style=\"padding: 10px\">\r\n            评价方式\r\n            <el-select v-model=\"scelOption.scl1\" placeholder=\"请选择\">\r\n              <el-option label=\"杭州市\" value=\"杭州市\"> </el-option>\r\n            </el-select>\r\n          </div>\r\n          <div style=\"padding: 10px\">\r\n            评价范围\r\n            <el-select v-model=\"scelOption.scl2\" placeholder=\"请选择\">\r\n              <el-option label=\"原始影像\" value=\"原始影像\"> </el-option>\r\n              <el-option label=\"辐射定标\" value=\"辐射定标\"> </el-option>\r\n              <el-option label=\"大气校正\" value=\"大气校正\"> </el-option>\r\n            </el-select>\r\n          </div>\r\n          <div style=\"padding: 10px\">\r\n            <el-button type=\"primary\" @click=\"changePngMap\"\r\n              >切换数据图层</el-button\r\n            >\r\n          </div>\r\n        </div>\r\n        <el-card\r\n          shadow=\"never\"\r\n          class=\"box-card\"\r\n          style=\"width: 120px; z-index: 9999\"\r\n        >\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>图层切换</span>\r\n          </div>\r\n          <el-radio-group v-model=\"mapType\" @change=\"mapChange\">\r\n            <el-radio label=\"normal\">标准</el-radio><br />\r\n            <el-radio label=\"dark\">幻影黑</el-radio><br />\r\n            <el-radio label=\"light\">月光银</el-radio><br />\r\n            <el-radio label=\"yg\">遥感图</el-radio>\r\n          </el-radio-group>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=2eccb47b400c8ab58f2dc596dbfe9d53&plugin=AMap.MouseTool,AMap.DistrictSearch\"\r\n></script>\r\n<script>\r\nimport hz1 from \"../assets/2013杭州原始影像0311.png\";\r\nimport hz2 from \"../assets/2013杭州辐射定标后0311.png\";\r\nimport hz3 from \"../assets/2013杭州大气校正后0311.png\";\r\nimport { mapState } from \"vuex\";\r\nexport default {\r\n  computed: {\r\n    ...mapState([\"markData2\"]),\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      markData: [],\r\n      dialogTableVisible: false,\r\n\r\n      mapType: \"normal\",\r\n      map: \"\",\r\n      marker: null, //点位\r\n      title: \"\",\r\n      content: [],\r\n      infoWindow: null,\r\n      markers: [],\r\n      satellite: null,\r\n\r\n      isPG: false,\r\n      scelOption: {\r\n        scl1: \"杭州市\",\r\n        scl2: \"原始影像\",\r\n      },\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.changePngMap();\r\n  },\r\n\r\n  methods: {\r\n    mapChange() {\r\n      if (this.mapType === \"yg\") {\r\n        this.satellite = new AMap.TileLayer.Satellite();\r\n        this.map.add(this.satellite);\r\n      } else {\r\n        if (this.satellite) {\r\n          this.map.remove(this.satellite);\r\n        }\r\n        var styleName = \"amap://styles/\" + this.mapType;\r\n        this.map.setMapStyle(styleName);\r\n      }\r\n    },\r\n\r\n    changePngMap() {\r\n      var imageLayer = new AMap.ImageLayer({\r\n        url: hz1,\r\n        bounds: new AMap.Bounds([118.83333, 29.75], [120.66667, 30.5833333]),\r\n      });\r\n\r\n      if (this.scelOption.scl2 === \"辐射定标\") {\r\n        imageLayer = new AMap.ImageLayer({\r\n          url: hz2,\r\n          bounds: new AMap.Bounds([118.83333, 29.75], [120.66667, 30.5833333]),\r\n        });\r\n      }\r\n\r\n      if (this.scelOption.scl2 === \"大气校正\") {\r\n        imageLayer = new AMap.ImageLayer({\r\n          url: hz3,\r\n          bounds: new AMap.Bounds([118.83333, 29.75], [120.66667, 30.5833333]),\r\n        });\r\n      }\r\n\r\n      this.map = new AMap.Map(\"container0307\", {\r\n        resizeEnable: true,\r\n        zoom: 9, //级别\r\n        zooms: [6.5, 30],\r\n        center: [120.210792, 30.246026], //中心点坐标\r\n        viewMode: \"2D\", //使用3D视图\r\n        mapStyle: \"amap://styles/normal\",\r\n        infoWindow: null,\r\n        layers: [new AMap.TileLayer(), imageLayer],\r\n      });\r\n\r\n      this.mapType = \"normal\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.data-manage {\r\n  background: #f7f7f7;\r\n}\r\n\r\n.tablecard2 {\r\n  height: calc(100vh - 151px);\r\n  background: #fff;\r\n  border: 1px solid #ccc;\r\n  z-index: 999;\r\n}\r\n.box-card {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 322px;\r\n  border-radius: 0;\r\n}\r\n.box-card2 {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0px;\r\n  border-radius: 0;\r\n  height: 140px;\r\n}\r\n</style>\r\n"]}]}