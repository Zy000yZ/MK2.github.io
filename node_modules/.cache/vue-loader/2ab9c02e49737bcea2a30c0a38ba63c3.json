e-column prop=\"username\" label=\"用户名\" width=\"180\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"password\" label=\"密码\"> </el-table-column>\r\n\r\n        <el-table-column label=\"操作\" align=\"center\" width=\"300\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"editMain(scope.row)\" size=\"small\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button type=\"text\" size=\"small\" @click=\"delMain(scope.row)\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\">\r\n      <el-form :model=\"form\">\r\n        <el-form-item label=\"用户名\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"密码\" :label-width=\"formLabelWidth\">\r\n          <el-input v-model=\"form.password\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editUser\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MainTop from \"../components/MainTop.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    MainTop,\r\n  },\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n\r\n      dialogFormVisible: false,\r\n      form: {\r\n        username: \"\",\r\n        password: \"\",\r\n        checkPass: \"\",\r\n      },\r\n      formLabelWidth: \"120px\",\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    this.getData();\r\n  },\r\n\r\n  methods: {\r\n    async getData() {\r\n      var res = await this.$axios.get(\"/getAllUsers/getAllUsers\");\r\n      this.tableData = res.data.data;\r\n      console.log(this.userList);\r\n    },\r\n\r\n    editMain(e) {\r\n      console.log(e);\r\n      if (e.username === \"admin\") {\r\n        this.$message.error(\"管理员信息不可以编辑\");\r\n      } else {\r\n        this.dialogFormVisible = true;\r\n        this.form = e;\r\n      }\r\n    },\r\n\r\n    async editUser() {\r\n      if (this.form.username !== \"\" && this.form.password !== \"\") {\r\n        console.log(this.form);\r\n        this.form.checkPass = this.form.username;\r\n\r\n        var res = null;\r\n        res = await this.$axios.post(\"/editUserData/editUserData\", this.form);\r\n        if (res.status === 200) {\r\n          this.getData();\r\n          this.dialogFormVisible = false;\r\n        }\r\n      } else {\r\n        this.$message.error(\"用户名或密码不可以为空\");\r\n      }\r\n    },\r\n\r\n    async delMain(e) {\r\n      console.log(e);\r\n\r\n      if (e.username === \"admin\") {\r\n        this.$message.error(\"管理员信息不可以删除\");\r\n      } else {\r\n        var res = null;\r\n        res = await this.$axios.post(\"/removeOneUserData/removeOneUserData\", {\r\n          _id: e._id,\r\n        });\r\n        if (res.status === 200) {\r\n          this.getData();\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.user-data-manage .mani {\r\n  padding: 15px;\r\n}\r\n</style>"]}]}