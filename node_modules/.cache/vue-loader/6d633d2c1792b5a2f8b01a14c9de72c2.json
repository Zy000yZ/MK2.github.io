;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"AddGreenlandMap.vue","sourceRoot":"src/views/Heritage","sourcesContent":["<template>\r\n  <div class=\"measure-map\">\r\n    <div class=\"map-box\">\r\n      <div class=\"map-buttons\" style=\"padding: 15px\">\r\n        <el-select\r\n          v-model=\"nowDistrict\"\r\n          placeholder=\"目标区域\"\r\n          style=\"margin-right: 15px; width: 90px\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in disOption\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n        <!-- <el-input\r\n          disabled\r\n          style=\"margin-right: 15px; width: 155px\"\r\n          v-model=\"markObject.start\"\r\n        ></el-input> -->\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"markObject.markName\"\r\n          placeholder=\"点位名称 / 编号\"\r\n        ></el-input>\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"markObject.address\"\r\n          placeholder=\"安装地址\"\r\n        ></el-input>\r\n        <el-select\r\n          v-model=\"markObject.property\"\r\n          placeholder=\"属性\"\r\n          style=\"margin-right: 15px; width: 110px\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in propertyOptions\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n         <el-select\r\n          v-model=\"markObject.cl\"\r\n          placeholder=\"材质\"\r\n          style=\"margin-right: 15px; width: 110px\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in CLstateOptions\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n        <el-select\r\n          v-model=\"markObject.state\"\r\n          placeholder=\"在网状态\"\r\n          style=\"margin-right: 15px; width: 110px\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in stateOptions\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"markObject.company\"\r\n          placeholder=\"产权单位\"\r\n        ></el-input>\r\n         <el-select\r\n          v-model=\"markObject.method\"\r\n          placeholder=\"维护方式\"\r\n          style=\"margin-right: 15px; width: 130px\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in options\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\"\r\n          >\r\n          </el-option>\r\n        </el-select>\r\n        <el-input\r\n          style=\"margin-right: 15px; width: 150px\"\r\n          v-model=\"markObject.admin\"\r\n          placeholder=\"负责人\"\r\n        ></el-input>\r\n        <br />\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          style=\"color: #5f8d4e\"\r\n          @click=\"addMark\"\r\n          class=\"map-btn map-element\"\r\n          >{{ pagType === \"add\" ? \"上传数据\" : \"确认编辑\" }}</el-button\r\n        >\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"addRoadLayer\"\r\n          v-show=\"!roadNetwork\"\r\n          class=\"map-btn map-element\"\r\n          >打开路网</el-button\r\n        >\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"removeRoadLayer\"\r\n          v-show=\"roadNetwork\"\r\n          class=\"map-btn map-element\"\r\n          >关闭路网</el-button\r\n        >\r\n\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"addSatellite\"\r\n          v-show=\"isOrdMap\"\r\n          class=\"map-btn map-element\"\r\n          >卫星地图</el-button\r\n        >\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"removeSatellite\"\r\n          v-show=\"!isOrdMap\"\r\n          class=\"map-btn map-element\"\r\n          >普通地图</el-button\r\n        >\r\n\r\n        <el-button\r\n          type=\"text\"\r\n          size=\"mini\"\r\n          @click=\"resetMap\"\r\n          class=\"map-btn map-element\"\r\n          >刷新</el-button\r\n        >\r\n      </div>\r\n      <div id=\"container\" class=\"container\"></div>\r\n      <div class=\"input-card\" style=\"width: 170px\">\r\n        <h4 style=\"margin-bottom: 10px; font-weight: 600\">选择点位坐标：</h4>\r\n        <button\r\n          class=\"btn\"\r\n          v-if=\"pagType === 'add'\"\r\n          @click=\"drawPolygon\"\r\n          style=\"margin-bottom: 5px\"\r\n        >\r\n          绘制坐标\r\n        </button>\r\n\r\n        <button\r\n          class=\"btn\"\r\n          v-if=\"pagType !== 'add'\"\r\n          @click=\"drawPolygon\"\r\n          style=\"margin-bottom: 5px\"\r\n        >\r\n          编辑坐标\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script\r\n  type=\"text/javascript\"\r\n  src=\"https://webapi.amap.com/maps?v=1.4.15&key=84f61226c5c3819011a303ef5c321e5f&plugin=AMap.MouseTool,AMap.HawkEye,AMap.DistrictSearch,AMap.Scale,AMap.ToolBar,AMap.ControlBar,AMap.ImageLayer\"\r\n></script>\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      map: null,\r\n      mouseTool: null,\r\n      geocoder: null,\r\n      radios: null,\r\n      overlays: null,\r\n\r\n      isOrdMap: true, //判断是卫星图还是普通图，普通图为true\r\n      satellite: null, //用来存储卫星地图影像\r\n      roadNetwork: false, //是否具有路网，默认没有\r\n      roadNetLayer: null, //用来存储路网\r\n\r\n      polyEditor: null,\r\n\r\n      // 停车场数据存储\r\n      mianji: 0,\r\n      pathArr: null,\r\n      inputName: null,\r\n      adddate: \"\",\r\n      parknum: 50,\r\n      parkmoney: 1,\r\n      parkadmin: \"\",\r\n      parktel: \"\",\r\n      valueType: null,\r\n\r\n      pagType: null,\r\n\r\n      dataList: null,\r\n      nowDistrict: \"黄浦区\",\r\n      polygons: [], //覆盖物数组\r\n      oneData: null,\r\n      polyEditor: null, //编辑多边形\r\n      isEditIng: false,\r\n\r\n      // 维护方式\r\n      options: [\r\n        {\r\n          value: \"自维\",\r\n          label: \"自维\",\r\n        },\r\n        {\r\n          value: \"第三方维护\",\r\n          label: \"第三方维护\",\r\n        },\r\n        {\r\n          value: \"其他\",\r\n          label: \"其他\",\r\n        },\r\n      ],\r\n      // 点位属性\r\n      propertyOptions: [\r\n        {\r\n          value: \"人井\",\r\n          label: \"人井\",\r\n        },\r\n        {\r\n          value: \"电杆\",\r\n          label: \"电杆\",\r\n        },\r\n                {\r\n          value: \"挂墙点\",\r\n          label: \"挂墙点\",\r\n        },\r\n        {\r\n          value: \"光交箱\",\r\n          label: \"光交箱\",\r\n        },\r\n        {\r\n          value: \"光分箱\",\r\n          label: \"光分箱\",\r\n        },\r\n        {\r\n          value: \"资源点\",\r\n          label: \"资源点\",\r\n        },\r\n        {\r\n          value: \"光缆引上点\",\r\n          label: \"光缆引上点\",\r\n        },\r\n      ],\r\n      // 在网状态\r\n      stateOptions: [\r\n        {\r\n          value: \"在网\",\r\n          label: \"在网\",\r\n        },\r\n        {\r\n          value: \"设计阶段\",\r\n          label: \"设计阶段\",\r\n        },\r\n        {\r\n          value: \"施工阶段\",\r\n          label: \"施工阶段\",\r\n        },\r\n      ],\r\n       CLstateOptions: [\r\n        {\r\n          value: \"铁铸\",\r\n          label: \"铁铸\",\r\n        },\r\n        {\r\n          value: \"水泥\",\r\n          label: \"水泥\",\r\n        },\r\n        {\r\n          value: \"复合\",\r\n          label: \"复合\",\r\n        },\r\n          {\r\n          value: \"其他\",\r\n          label: \"其他\",\r\n        },\r\n      ],\r\n\r\n      markObject: {\r\n        markName: \"\",\r\n        address: \"\",\r\n        method: \"自维\", //维护方式\r\n        property: \"人井\", //属性\r\n        state: \"在网\", //在网状态\r\n        start: \"\", //建设日期\r\n        editData: \"\", //数据更新日期\r\n        company: \"自用\",\r\n        cl:'铁铸',\r\n        admin: \"\",\r\n        jing: \"\",\r\n        wei: \"\",\r\n        addressObject: {\r\n          district: \"黄浦区\",\r\n        },\r\n        lineArr: [],\r\n      },\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    // console.log(this.$route);\r\n    const now = new Date();\r\n    const year = now.getFullYear();\r\n    const month = now.getMonth() + 1; // 月份从0开始，需要加1\r\n    const day = now.getDate();\r\n    const hours = now.getHours(); // 获取当前小时，例如：15\r\n    const minutes = now.getMinutes(); // 获取当前分钟，例如：30\r\n    const seconds = now.getSeconds(); // 获取当前秒数，例如：45\r\n    this.markObject.editData =\r\n      year +\r\n      \"/\" +\r\n      month +\r\n      \"/\" +\r\n      day +\r\n      \" \" +\r\n      hours +\r\n      \":\" +\r\n      minutes +\r\n      \":\" +\r\n      seconds;\r\n\r\n    this.pagType = this.$route.params.editid;\r\n    if (this.pagType === \"add\") {\r\n      this.markObject.start =\r\n        year +\r\n        \"/\" +\r\n        month +\r\n        \"/\" +\r\n        day +\r\n        \" \" +\r\n        hours +\r\n        \":\" +\r\n        minutes +\r\n        \":\" +\r\n        seconds;\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState([\"disOption\", \"cityCode\"]),\r\n  },\r\n\r\n  mounted() {\r\n    this.initMap();\r\n    this.getQuPointData();\r\n\r\n    if (this.pagType === \"add\") {\r\n    } else {\r\n      this.addEditPol();\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    initMap() {\r\n      this.map = new AMap.Map(\"container\", {\r\n        resizeEnable: true,\r\n        zoom: 14, //级别\r\n        zooms: [3, 30],\r\n        center: [121.47, 31.23], //中心点坐标\r\n        viewMode: \"3D\", //使用3D视图\r\n        lang: this.lang,\r\n        mapStyle: \"amap://styles/macaron\",\r\n        infoWindow: null,\r\n      });\r\n      this.mouseTool = new AMap.MouseTool(this.map);\r\n      this.geocoder = new AMap.Geocoder({\r\n        radius: 1000, //范围，默认：500\r\n      });\r\n    },\r\n\r\n    async addEditPol() {\r\n      var res = null;\r\n      res = await this.$axios.post(\"/LvDiDataTableGetOne/LvDiDataTableGetOne\", {\r\n        _id: this.pagType,\r\n      });\r\n\r\n      var zb = res.data.data[0].path.map((ele) => [ele.R, ele.Q]);\r\n\r\n      var polygon = new AMap.Polygon({\r\n        path: zb,\r\n        strokeColor: \"#FF33FF\",\r\n        strokeWeight: 6,\r\n        strokeOpacity: 0.2,\r\n        fillOpacity: 0.4,\r\n        fillColor: \"#1791fc\",\r\n        zIndex: 50,\r\n      });\r\n\r\n      this.map.add(polygon);\r\n      this.map.setFitView([polygon]);\r\n\r\n      this.polyEditor = new AMap.PolyEditor(this.map, polygon);\r\n\r\n      this.polyEditor.on(\"end\", (event) => {\r\n        // log.info(\"触发事件： end\");\r\n        console.log(\"end\", event);\r\n        this.mianji = event.target.getArea();\r\n        this.pathArr = event.target.getPath();\r\n        // event.target 即为编辑后的多边形对象\r\n      });\r\n    },\r\n\r\n    async addMark() {\r\n      if (!this.markObject.markName || !this.markObject.jing) {\r\n        this.$message.error(\"请先选择点位坐标\");\r\n      } else if (\r\n        !this.markObject.company ||\r\n        !this.markObject.admin ||\r\n        !this.markObject.markName\r\n      ) {\r\n        this.$message.error(\"请先完善点位相关信息\");\r\n      } else {\r\n        if (this.pagType === \"add\") {\r\n          // var qucode = this.cityCode.find(\r\n          //   (ele) => ele.qu === this.markObject.addressObject.district\r\n          // );\r\n          var res = null;\r\n          var str = \"/LanPoint\" + '01' + \"Add\";\r\n          res = await this.$axios.post(str + str, this.markObject);\r\n          if (res.status === 200) {\r\n            this.$message({\r\n              message: this.markObject.property + \"信息添加成功\",\r\n              type: \"success\",\r\n            });\r\n            this.$router.push(\"/data/manage\");\r\n          }\r\n        } else {\r\n          // 提交编辑\r\n          var res2 = null;\r\n          res2 = await this.$axios.post(\r\n            \"/LvDiDataTableEditData/LvDiDataTableEditData\",\r\n            {\r\n              name: this.inputName,\r\n              type: this.valueType,\r\n              path: this.pathArr,\r\n              mianji: this.mianji,\r\n              _id: this.pagType,\r\n            }\r\n          );\r\n          // console.log();\r\n          if (res2.status === 200) {\r\n            this.$message({\r\n              message: \"信息编辑成功\",\r\n              type: \"success\",\r\n            });\r\n\r\n            this.$router.push(\"/data/manage\");\r\n          }\r\n        }\r\n      }\r\n    },\r\n\r\n    // 获取当前区域点数据\r\n    async getQuPointData() {\r\n      var res = null;\r\n      var str = \"/LanPoint\" + '01' + \"Get\";\r\n      res = await this.$axios.get(str + str);\r\n      if (res.status === 200) {\r\n        console.log(\"success\", this.nowDistrict, res);\r\n        this.dataList = res.data.data;\r\n        this.addMarker();\r\n        this.map.setFitView();\r\n      }\r\n    },\r\n    // 添加点集\r\n    addMarker() {\r\n      this.map.clearMap();\r\n      this.markers = [];\r\n      for (let i = 0; i < this.dataList.length; i++) {\r\n        // 注意用 let\r\n        var url =\r\n          \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/资源点.png\";\r\n        if (this.dataList[i].property === \"人井\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/人井0812.png\";\r\n        }\r\n        if (this.dataList[i].property === \"电杆\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/电杆0818.png\";\r\n        }\r\n        if (this.dataList[i].property === \"光缆引上点\") {\r\n          url =\r\n            \"https://mp-b0bd9cd4-ad06-4cf0-b1a3-ea2e46e926bc.cdn.bspapp.com/cloudstorage/光缆引上点.png\";\r\n        }\r\n\r\n        let marker = new AMap.Marker({\r\n          icon: new AMap.Icon({\r\n            image: url,\r\n            size: new AMap.Size(30, 30),\r\n            imageSize: new AMap.Size(30, 30),\r\n          }),\r\n          map: this.map,\r\n          position: [this.dataList[i].jing, this.dataList[i].wei],\r\n          extData: this.dataList[i],\r\n        });\r\n        this.markers.push(marker);\r\n        AMap.event.addListener(marker, \"click\", () => {\r\n          // console.log('图标被点击了')\r\n          this.title = this.dataList[i].markName;\r\n          (this.content = [\r\n            \"安装地址：\" + this.dataList[i].address,\r\n            \"更新时间：\" + this.dataList[i].editData,\r\n            \"产权单位：\" + this.dataList[i].company,\r\n            \"负责人：\" + this.dataList[i].admin,\r\n          ]),\r\n            (this.infoWindow = new AMap.InfoWindow({\r\n              isCustom: true, //使用自定义窗体\r\n              content: this.createInfoWindow(\r\n                this.title,\r\n                this.content.join(\"<br/>\")\r\n              ),\r\n              offset: new AMap.Pixel(16, -45),\r\n            }));\r\n          this.infoWindow.open(this.map, marker.getPosition());\r\n        });\r\n      }\r\n    },\r\n\r\n    //渲染弹框\r\n    createInfoWindow(title, content) {\r\n      var info = document.createElement(\"div\");\r\n      info.className = \"custom-info input-card content-window-card\";\r\n      //可以通过下面的方式修改自定义窗体的宽高\r\n      //info.style.width = \"400px\";\r\n      // 定义顶部标题\r\n      var top = document.createElement(\"div\");\r\n      var titleD = document.createElement(\"div\");\r\n      var closeX = document.createElement(\"img\");\r\n      top.className = \"info-top\";\r\n      titleD.innerHTML = title;\r\n      closeX.src = \"https://webapi.amap.com/images/close2.gif\";\r\n      closeX.onclick = this.closeInfoWindow;\r\n\r\n      top.appendChild(titleD);\r\n      top.appendChild(closeX);\r\n      info.appendChild(top);\r\n\r\n      // 定义中部内容\r\n      var middle = document.createElement(\"div\");\r\n      middle.className = \"info-middle\";\r\n      middle.style.backgroundColor = \"white\";\r\n      middle.innerHTML = content;\r\n      info.appendChild(middle);\r\n\r\n      // 定义底部内容\r\n      var bottom = document.createElement(\"div\");\r\n      bottom.className = \"info-bottom\";\r\n      bottom.style.position = \"relative\";\r\n      bottom.style.top = \"0px\";\r\n      bottom.style.margin = \"0 auto\";\r\n      var sharp = document.createElement(\"img\");\r\n      sharp.src = \"https://webapi.amap.com/images/sharp.png\";\r\n      bottom.appendChild(sharp);\r\n      info.appendChild(bottom);\r\n      return info;\r\n    },\r\n    // 关闭弹框\r\n    closeInfoWindow() {\r\n      this.map.clearInfoWindow();\r\n      this.searchIndustry = null;\r\n    },\r\n\r\n    drawPolygon() {\r\n      // this.map.clearMap();\r\n      this.mouseTool.marker({});\r\n      if (this.overlays) {\r\n        this.map.remove(this.overlays);\r\n      }\r\n      // this.addMarker()\r\n      this.mouseTool.on(\"draw\", (e) => {\r\n        this.overlays = e.obj;\r\n        var zb = e.obj.getPosition();\r\n        // console.log('绘制的坐标',[zb.lng, zb.lat]);\r\n        this.markObject.jing = zb.lng;\r\n        this.markObject.wei = zb.lat;\r\n        this.mouseTool.close(); //关闭\r\n\r\n        this.geocoder.getAddress([zb.lng, zb.lat], async (status, result) => {\r\n          if (status === \"complete\" && result.regeocode) {\r\n            this.markObject.address = result.regeocode.formattedAddress;\r\n            this.markObject.addressObject.adcode =\r\n              result.regeocode.addressComponent.adcode;\r\n            this.markObject.addressObject.district =\r\n              result.regeocode.addressComponent.district;\r\n            this.markObject.addressObject.province =\r\n              result.regeocode.addressComponent.province;\r\n            this.markObject.addressObject.street =\r\n              result.regeocode.addressComponent.street;\r\n            this.markObject.addressObject.streetNumber =\r\n              result.regeocode.addressComponent.streetNumber;\r\n            this.markObject.addressObject.township =\r\n              result.regeocode.addressComponent.township;\r\n            this.markObject.addressObject.towncode =\r\n              result.regeocode.addressComponent.towncode;\r\n\r\n            // 根据区域发送请求获取点位长度\r\n            // var qucode = this.cityCode.find(\r\n            //   (ele) => ele.qu === result.regeocode.addressComponent.district\r\n            // );\r\n            // var res = null;\r\n            // var str = \"/LanPoint\" + qucode.code + \"Get\";\r\n            // res = await this.$axios.get(str + str);\r\n            // if (res.status === 200) {\r\n            //   this.markObject.markName =\r\n            //     result.regeocode.addressComponent.district +\r\n            //     \" - \" +\r\n            //     this.markObject.property +\r\n            //     \" - \" +\r\n            //     (res.data.data.length + 1);\r\n            //   // console.log(\"success\", result, res);\r\n            // }\r\n          } else {\r\n            alert(\"根据经纬度查询地址失败\");\r\n          }\r\n        });\r\n      }); //解除绘制绑定，\r\n    },\r\n\r\n    // 打开卫星地图\r\n    addSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.satellite = new AMap.TileLayer.Satellite();\r\n      this.map.add(this.satellite);\r\n    },\r\n\r\n    // 关闭卫星地图\r\n    removeSatellite() {\r\n      this.isOrdMap = !this.isOrdMap;\r\n      this.map.remove(this.satellite);\r\n    },\r\n\r\n    // 打开路网\r\n    addRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.roadNetLayer = new AMap.TileLayer.RoadNet();\r\n      this.map.add(this.roadNetLayer);\r\n    },\r\n\r\n    // 关闭路网\r\n    removeRoadLayer() {\r\n      this.roadNetwork = !this.roadNetwork;\r\n      this.map.remove(this.roadNetLayer);\r\n    },\r\n\r\n    resetMap() {\r\n      location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.btn {\r\n  width: 6rem;\r\n  margin: 0 1rem 0 4.5rem;\r\n}\r\n.btn {\r\n  display: inline-block;\r\n  font-weight: 400;\r\n  text-align: center;\r\n  white-space: nowrap;\r\n  vertical-align: middle;\r\n  -webkit-user-select: none;\r\n  -moz-user-select: none;\r\n  -ms-user-select: none;\r\n  user-select: none;\r\n  border: 1px solid transparent;\r\n  transition: color 0.15s ease-in-out, background-color 0.15s ease-in-out,\r\n    border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;\r\n  background-color: transparent;\r\n  background-image: none;\r\n  color: #25a5f7;\r\n  border-color: #25a5f7;\r\n  padding: 0.25rem 0.5rem;\r\n  line-height: 1.5;\r\n  border-radius: 1rem;\r\n  -webkit-appearance: button;\r\n  cursor: pointer;\r\n}\r\n.btn:hover {\r\n  color: #fff;\r\n  background-color: #25a5f7;\r\n  border-color: #25a5f7;\r\n}\r\n\r\n.btn:hover {\r\n  text-decoration: none;\r\n}\r\n\r\n.input-card {\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-width: 0;\r\n  word-wrap: break-word;\r\n  background-color: #fff;\r\n  background-clip: border-box;\r\n  border-radius: 0.25rem;\r\n  width: 14rem;\r\n  border-width: 0;\r\n  border-radius: 0.4rem;\r\n  box-shadow: 0 2px 6px 0 rgba(114, 124, 245, 0.5);\r\n  position: absolute;\r\n  bottom: 2rem;\r\n  right: 2rem;\r\n  -ms-flex: 1 1 auto;\r\n  flex: 1 1 auto;\r\n  padding: 0.75rem 1.25rem;\r\n}\r\n/* 弹框样式 */\r\n.content-window-card {\r\n  position: relative;\r\n  box-shadow: none;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: auto;\r\n  padding: 0;\r\n}\r\n\r\n.content-window-card p {\r\n  height: 2rem;\r\n}\r\n\r\n.custom-info {\r\n  border: solid 1px silver;\r\n  width: 280px;\r\n  background-color: white;\r\n}\r\n\r\ndiv.info-top {\r\n  position: relative;\r\n  background: none repeat scroll 0 0 #f9f9f9;\r\n  border-bottom: 1px solid #ccc;\r\n  border-radius: 5px 5px 0 0;\r\n}\r\n\r\ndiv.info-top div {\r\n  display: inline-block;\r\n  color: #333333;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  line-height: 31px;\r\n  padding: 0 10px;\r\n}\r\n\r\ndiv.info-top img {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  transition-duration: 0.25s;\r\n}\r\n\r\ndiv.info-middle {\r\n  font-size: 12px;\r\n  padding: 10px 6px;\r\n  line-height: 20px;\r\n}\r\ndiv.info-middle img {\r\n  width: 100px;\r\n  height: 70px;\r\n  padding-bottom: 10px;\r\n}\r\n\r\ndiv.info-bottom {\r\n  height: 0px;\r\n  width: 100%;\r\n  clear: both;\r\n  text-align: center;\r\n}\r\n\r\ndiv.info-bottom img {\r\n  position: relative;\r\n  z-index: 104;\r\n}\r\n\r\nspan {\r\n  margin-left: 5px;\r\n  font-size: 11px;\r\n}\r\n\r\n.info-middle img {\r\n  float: left;\r\n  margin-right: 6px;\r\n}\r\n\r\n.xiangxi {\r\n  color: #555;\r\n}\r\n</style>\r\n"]}]}